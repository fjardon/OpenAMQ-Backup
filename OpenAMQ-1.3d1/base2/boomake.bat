:---------------------   BUILD PRODUCT INSTALL PACKAGES   ---------------------

:a_build_product_install_packages
    echo boom I: [base2]: building Base2 Install...
    if exist _install\nul rmdir /s /q _install
    mkdir _install
    mkdir _install\lib
    mkdir _install\include
    mkdir _install\bin
    pushd xnf
    if exist boomake.bat (
    call boomake -install ..\_install install
    ) else (
        echo boom E: [base2]: project is not configured - run 'boom configure'
    )
    popd
    pushd icl
    if exist boomake.bat (
    call boomake -install ..\_install install
    ) else (
        echo boom E: [base2]: project is not configured - run 'boom configure'
    )
    popd
    pushd ipr
    if exist boomake.bat (
    call boomake -install ..\_install install
    ) else (
        echo boom E: [base2]: project is not configured - run 'boom configure'
    )
    popd
    pushd opf
    if exist boomake.bat (
    call boomake -install ..\_install install
    ) else (
        echo boom E: [base2]: project is not configured - run 'boom configure'
    )
    popd
    pushd smt
    if exist boomake.bat (
    call boomake -install ..\_install install
    ) else (
        echo boom E: [base2]: project is not configured - run 'boom configure'
    )
    popd
    pushd asl
    if exist boomake.bat (
    call boomake -install ..\_install install
    ) else (
        echo boom E: [base2]: project is not configured - run 'boom configure'
    )
    popd
    if exist base2-!MY_VERSION! rmdir /s/q base2-!MY_VERSION!
    ren _install base2-!MY_VERSION!
    if "!BOOM_TRACE!"=="1" echo pkg_zip base2-!MY_VERSION!
    verify>nul
    call pkg_zip base2-!MY_VERSION!
    if errorlevel 1 (
        echo boom E: [base2]: 'pkg_zip base2-!MY_VERSION!' failed - stop.
        set _exception=1
        goto :eof
    )
    rmdir /s/q base2-!MY_VERSION!
    goto :eof

