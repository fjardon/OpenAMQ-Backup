    if [ "$BOOM_MODEL_CPP" ]; then
    [ "$BOOM_TRACE" ] && echo "cat ipr_sasl.hpp>>ipr.hpp"
    cat ipr_sasl.hpp>>ipr.hpp || croak "\"cat ipr_sasl.hpp>>ipr.hpp\" failed"
    fi
    if [ "$BOOM_MODEL_CPP" ]; then
    [ "$BOOM_TRACE" ] && echo "cat ipr_stat.hpp>>ipr.hpp"
    cat ipr_stat.hpp>>ipr.hpp || croak "\"cat ipr_stat.hpp>>ipr.hpp\" failed"
    fi
    if [ "$BOOM_MODEL_CPP" ]; then
    [ "$BOOM_TRACE" ] && echo "cat ipr_str.hpp>>ipr.hpp"
    cat ipr_str.hpp>>ipr.hpp || croak "\"cat ipr_str.hpp>>ipr.hpp\" failed"
    fi
    if [ "$BOOM_MODEL_CPP" ]; then
    [ "$BOOM_TRACE" ] && echo "cat ipr_symbol.hpp>>ipr.hpp"
    cat ipr_symbol.hpp>>ipr.hpp || croak "\"cat ipr_symbol.hpp>>ipr.hpp\" failed"
    fi
    if [ "$BOOM_MODEL_CPP" ]; then
    [ "$BOOM_TRACE" ] && echo "cat ipr_token.hpp>>ipr.hpp"
    cat ipr_token.hpp>>ipr.hpp || croak "\"cat ipr_token.hpp>>ipr.hpp\" failed"
    fi
    if [ "$BOOM_MODEL_CPP" ]; then
    [ "$BOOM_TRACE" ] && echo "cat ipr_xml.hpp>>ipr.hpp"
    cat ipr_xml.hpp>>ipr.hpp || croak "\"cat ipr_xml.hpp>>ipr.hpp\" failed"
    fi
    if [ "$BOOM_MODEL_CPP" ]; then
    [ "$BOOM_TRACE" ] && echo "cat ipr_uid.hpp>>ipr.hpp"
    cat ipr_uid.hpp>>ipr.hpp || croak "\"cat ipr_uid.hpp>>ipr.hpp\" failed"
    fi
    if [ "$BOOM_MODEL_CPP" ]; then
    [ "$BOOM_TRACE" ] && echo "cat ipr_time.hpp>>ipr.hpp"
    cat ipr_time.hpp>>ipr.hpp || croak "\"cat ipr_time.hpp>>ipr.hpp\" failed"
    fi
    echo "Source files successfully generated">stamp_generate
}

