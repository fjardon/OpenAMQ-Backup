    [ "$BOOM_TRACE" ] && echo "cat ipr_stat.hpp>>ipr.hpp"
    cat ipr_stat.hpp>>ipr.hpp || croak "\"cat ipr_stat.hpp>>ipr.hpp\" failed"
    [ "$BOOM_TRACE" ] && echo "cat ipr_str.hpp>>ipr.hpp"
    cat ipr_str.hpp>>ipr.hpp || croak "\"cat ipr_str.hpp>>ipr.hpp\" failed"
