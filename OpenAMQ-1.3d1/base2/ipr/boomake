    fi
    if [ ! -f "ipr_stat.h" ]; then
        croak "ipr_stat.h is missing"
    fi
    if [ "$BOOM_MODEL_CPP" ]; then
    if [ ! -f "ipr_stat.hpp" ]; then
        croak "ipr_stat.hpp is missing"
    fi
    fi
    if [ ! -f "ipr_str.h" ]; then
        croak "ipr_str.h is missing"
    fi
    if [ "$BOOM_MODEL_CPP" ]; then
    if [ ! -f "ipr_str.hpp" ]; then
        croak "ipr_str.hpp is missing"
    fi
    fi
    if [ ! -f "ipr_uid.h" ]; then
        croak "ipr_uid.h is missing"
    fi
    if [ "$BOOM_MODEL_CPP" ]; then
    if [ ! -f "ipr_uid.hpp" ]; then
        croak "ipr_uid.hpp is missing"
    fi
    fi
    if [ ! -f "ipr_time.h" ]; then
        croak "ipr_time.h is missing"
    fi
    if [ "$BOOM_MODEL_CPP" ]; then
    if [ ! -f "ipr_time.hpp" ]; then
        croak "ipr_time.hpp is missing"
    fi
    fi
    if [ ! -f "ipr_xml.h" ]; then
        croak "ipr_xml.h is missing"
    fi
    if [ "$BOOM_MODEL_CPP" ]; then
    if [ ! -f "ipr_xml.hpp" ]; then
        croak "ipr_xml.hpp is missing"
    fi
    fi
    if [ ! -f "ipr_xml_list.h" ]; then
        croak "ipr_xml_list.h is missing"
    fi
    if [ "$BOOM_MODEL_CPP" ]; then
    if [ ! -f "ipr_xml_list.hpp" ]; then
        croak "ipr_xml_list.hpp is missing"
    fi
    fi
    if [ ! -f "ipr_xml_attr.h" ]; then
        croak "ipr_xml_attr.h is missing"
    fi
    if [ "$BOOM_MODEL_CPP" ]; then
    if [ ! -f "ipr_xml_attr.hpp" ]; then
        croak "ipr_xml_attr.hpp is missing"
    fi
    fi
    if [ ! -f "ipr_xml_attr_list.h" ]; then
        croak "ipr_xml_attr_list.h is missing"
    fi
    if [ "$BOOM_MODEL_CPP" ]; then
    if [ ! -f "ipr_xml_attr_list.hpp" ]; then
        croak "ipr_xml_attr_list.hpp is missing"
    fi
    fi
    if [ "$BOOM_MODEL_CPP" ]; then
    if [ ! -f "ipr.hpp" ]; then
        croak "ipr.hpp is missing"
    fi
    fi
}

# ------------------------   CHECK OPERATING CONTEXT   ------------------------

a_check_operating_context() {
:
    if [ -f ./stamp_stripped ]; then
        event=stripped
    elif [ -f ./stamp_source ]; then
        event=source
    else
        event=repository
    fi
}

# -----------------------   CHECK PROJECT FILES EXIST   -----------------------

a_check_project_files_exist() {
:
    if [ ! -f "version.h" ]; then
        croak "version.h is missing"
    fi
    if [ ! -f "ipr_bits.icl" ]; then
        croak "ipr_bits.icl is missing"
    fi
    if [ ! -f "ipr_bucket.icl" ]; then
        croak "ipr_bucket.icl is missing"
