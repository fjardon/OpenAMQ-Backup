    cat ipr_uid.hpp>>ipr.hpp || croak "\"cat ipr_uid.hpp>>ipr.hpp\" failed"
    [ "$BOOM_TRACE" ] && echo "cat ipr_time.hpp>>ipr.hpp"
    cat ipr_time.hpp>>ipr.hpp || croak "\"cat ipr_time.hpp>>ipr.hpp\" failed"
    echo "Source files successfully generated">stamp_generate
}

# ------------------------   PRODUCE GENERATED FILES   ------------------------

a_produce_generated_files() {
:
    carp "Generating files..."
    if [ "$BOOM_MODEL_CPP" ]; then
    [ "$BOOM_TRACE" ] && echo "gsl -q -quiet:1 -target:stdcpp ipr_bits.icl"
    gsl -q -quiet:1 -target:stdcpp ipr_bits.icl || croak "\"gsl -q -quiet:1 -target:stdcpp ipr_bits.icl\" failed"
    fi
    [ "$BOOM_TRACE" ] && echo "gsl -q -quiet:1 ipr_bits.icl"
    gsl -q -quiet:1 ipr_bits.icl || croak "\"gsl -q -quiet:1 ipr_bits.icl\" failed"
    if [ "$BOOM_MODEL_CPP" ]; then
    [ "$BOOM_TRACE" ] && echo "gsl -q -quiet:1 -target:stdcpp ipr_bucket.icl"
    gsl -q -quiet:1 -target:stdcpp ipr_bucket.icl || croak "\"gsl -q -quiet:1 -target:stdcpp ipr_bucket.icl\" failed"
    fi
    [ "$BOOM_TRACE" ] && echo "gsl -q -quiet:1 ipr_bucket.icl"
    gsl -q -quiet:1 ipr_bucket.icl || croak "\"gsl -q -quiet:1 ipr_bucket.icl\" failed"
    if [ "$BOOM_MODEL_CPP" ]; then
    [ "$BOOM_TRACE" ] && echo "gsl -q -quiet:1 -target:stdcpp ipr_bucket_list.icl"
    gsl -q -quiet:1 -target:stdcpp ipr_bucket_list.icl || croak "\"gsl -q -quiet:1 -target:stdcpp ipr_bucket_list.icl\" failed"
    fi
    [ "$BOOM_TRACE" ] && echo "gsl -q -quiet:1 ipr_bucket_list.icl"
    gsl -q -quiet:1 ipr_bucket_list.icl || croak "\"gsl -q -quiet:1 ipr_bucket_list.icl\" failed"
    if [ "$BOOM_MODEL_CPP" ]; then
    [ "$BOOM_TRACE" ] && echo "gsl -q -quiet:1 -target:stdcpp ipr_mother.icl"
    gsl -q -quiet:1 -target:stdcpp ipr_mother.icl || croak "\"gsl -q -quiet:1 -target:stdcpp ipr_mother.icl\" failed"
    fi
    [ "$BOOM_TRACE" ] && echo "gsl -q -quiet:1 ipr_mother.icl"
    gsl -q -quiet:1 ipr_mother.icl || croak "\"gsl -q -quiet:1 ipr_mother.icl\" failed"
    if [ "$BOOM_MODEL_CPP" ]; then
    [ "$BOOM_TRACE" ] && echo "gsl -q -quiet:1 -target:stdcpp ipr_config.icl"
    gsl -q -quiet:1 -target:stdcpp ipr_config.icl || croak "\"gsl -q -quiet:1 -target:stdcpp ipr_config.icl\" failed"
    fi
    [ "$BOOM_TRACE" ] && echo "gsl -q -quiet:1 ipr_config.icl"
    gsl -q -quiet:1 ipr_config.icl || croak "\"gsl -q -quiet:1 ipr_config.icl\" failed"
    if [ "$BOOM_MODEL_CPP" ]; then
    [ "$BOOM_TRACE" ] && echo "gsl -q -quiet:1 -target:stdcpp ipr_crc.icl"
