    [ "$BOOM_TRACE" ] && echo "cat ipr_hash.hpp>>ipr.hpp"
    cat ipr_hash.hpp>>ipr.hpp || croak "\"cat ipr_hash.hpp>>ipr.hpp\" failed"
    [ "$BOOM_TRACE" ] && echo "cat ipr_index.hpp>>ipr.hpp"
    cat ipr_index.hpp>>ipr.hpp || croak "\"cat ipr_index.hpp>>ipr.hpp\" failed"
    [ "$BOOM_TRACE" ] && echo "cat ipr_log.hpp>>ipr.hpp"
    cat ipr_log.hpp>>ipr.hpp || croak "\"cat ipr_log.hpp>>ipr.hpp\" failed"
