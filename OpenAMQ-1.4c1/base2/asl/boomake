a_produce_generated_files() {
:
    carp "Generating files..."
    [ "$BOOM_TRACE" ] && echo "gsl -q -quiet:1 asl.xnf"
    gsl -q -quiet:1 asl.xnf || croak "\"gsl -q -quiet:1 asl.xnf\" failed"
    [ "$BOOM_TRACE" ] && echo "gsl -q -quiet:1 asl_field.icl"
