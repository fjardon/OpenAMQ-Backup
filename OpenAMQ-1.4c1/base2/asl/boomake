    [ "$BOOM_TRACE" ] && echo "gsl -q -quiet:1 demo_client_proto.asl"
    gsl -q -quiet:1 demo_client_proto.asl || croak "\"gsl -q -quiet:1 demo_client_proto.asl\" failed"
    [ "$BOOM_TRACE" ] && echo "gsl -q -quiet:1 demo_client_method.icl"
