    chmod 0644 $_INSTALL_ROOT/include/demo_queue_agent.h || croak "\"chmod 0644 $_INSTALL_ROOT/include/demo_queue_agent.h\" failed"
    [ "$BOOM_TRACE" ] && echo "cp demo_lease.h $_INSTALL_ROOT/include/"
