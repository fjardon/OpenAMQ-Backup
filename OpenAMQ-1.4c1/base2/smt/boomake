    [ "$BOOM_TRACE" ] && echo "c $MY_COPTS -r libsmt smt_demo_server"
    c $MY_COPTS -r libsmt smt_demo_server || croak "\"c $MY_COPTS -r libsmt smt_demo_server\" failed"
