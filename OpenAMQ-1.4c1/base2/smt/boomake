        croak "smt_log.c is missing"
    fi
    if [ ! -f "smt_demo_portal_test.c" ]; then
        croak "smt_demo_portal_test.c is missing"
    fi
    if [ ! -f "smt_demo_portal.h" ]; then
        croak "smt_demo_portal.h is missing"
    fi
