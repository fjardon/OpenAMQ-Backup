    [ "$BOOM_TRACE" ] && echo "c $MY_COPTS smt_os_thread_by_status_test"
    c $MY_COPTS smt_os_thread_by_status_test || croak "\"c $MY_COPTS smt_os_thread_by_status_test\" failed"
