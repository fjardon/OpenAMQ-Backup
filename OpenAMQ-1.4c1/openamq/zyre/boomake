    gsl -q -quiet:1 zyre_amqp_join.icl || croak "\"gsl -q -quiet:1 zyre_amqp_join.icl\" failed"
    [ "$BOOM_TRACE" ] && echo "gsl -q -quiet:1 zyre_backend_amqp.icl"
    gsl -q -quiet:1 zyre_backend_amqp.icl || croak "\"gsl -q -quiet:1 zyre_backend_amqp.icl\" failed"
    [ "$BOOM_TRACE" ] && echo "gsl -q -quiet:1 zyre_digest_amqp.icl"
    gsl -q -quiet:1 zyre_digest_amqp.icl || croak "\"gsl -q -quiet:1 zyre_digest_amqp.icl\" failed"
    [ "$BOOM_TRACE" ] && echo "gsl -q -quiet:1 zyre_peer.asl"
    gsl -q -quiet:1 zyre_peer.asl || croak "\"gsl -q -quiet:1 zyre_peer.asl\" failed"
    [ "$BOOM_TRACE" ] && echo "gsl -q -quiet:1 zyre_peer_method.icl"
    gsl -q -quiet:1 zyre_peer_method.icl || croak "\"gsl -q -quiet:1 zyre_peer_method.icl\" failed"
    [ "$BOOM_TRACE" ] && echo "gsl -q -quiet:1 zyre_peer_config.opf"
    gsl -q -quiet:1 zyre_peer_config.opf || croak "\"gsl -q -quiet:1 zyre_peer_config.opf\" failed"
    [ "$BOOM_TRACE" ] && echo "gsl -q -quiet:1 zyre_peer_config.icl"
    gsl -q -quiet:1 zyre_peer_config.icl || croak "\"gsl -q -quiet:1 zyre_peer_config.icl\" failed"
    [ "$BOOM_TRACE" ] && echo "gsl -q -quiet:1 zyre_peer_agent.smt"
    gsl -q -quiet:1 zyre_peer_agent.smt || croak "\"gsl -q -quiet:1 zyre_peer_agent.smt\" failed"
    [ "$BOOM_TRACE" ] && echo "gsl -q -quiet:1 zyre_classes.icl"
    gsl -q -quiet:1 zyre_classes.icl || croak "\"gsl -q -quiet:1 zyre_classes.icl\" failed"
    [ "$BOOM_TRACE" ] && echo "perl -S ipr_resource.pl -@ zyre_static.rc"
    perl -S ipr_resource.pl -@ zyre_static.rc || croak "\"perl -S ipr_resource.pl -@ zyre_static.rc\" failed"
    [ "$BOOM_TRACE" ] && echo "gsl -q -quiet:1 zyre_restms_agent.smt"
    gsl -q -quiet:1 zyre_restms_agent.smt || croak "\"gsl -q -quiet:1 zyre_restms_agent.smt\" failed"
