    chmod 0644 $_INSTALL_ROOT/bin/zyre_amqp_join.icl || croak "\"chmod 0644 $_INSTALL_ROOT/bin/zyre_amqp_join.icl\" failed"
    [ "$BOOM_TRACE" ] && echo "cp zyre_peer_method.icl $_INSTALL_ROOT/bin/"
    cp zyre_peer_method.icl $_INSTALL_ROOT/bin/ || croak "\"cp zyre_peer_method.icl $_INSTALL_ROOT/bin/\" failed"
    [ "$BOOM_TRACE" ] && echo "chmod 0644 $_INSTALL_ROOT/bin/zyre_peer_method.icl"
    chmod 0644 $_INSTALL_ROOT/bin/zyre_peer_method.icl || croak "\"chmod 0644 $_INSTALL_ROOT/bin/zyre_peer_method.icl\" failed"
    [ "$BOOM_TRACE" ] && echo "cp zyre_peer_config.txt $_INSTALL_ROOT/bin/"
    cp zyre_peer_config.txt $_INSTALL_ROOT/bin/ || croak "\"cp zyre_peer_config.txt $_INSTALL_ROOT/bin/\" failed"
    [ "$BOOM_TRACE" ] && echo "chmod 0644 $_INSTALL_ROOT/bin/zyre_peer_config.txt"
    chmod 0644 $_INSTALL_ROOT/bin/zyre_peer_config.txt || croak "\"chmod 0644 $_INSTALL_ROOT/bin/zyre_peer_config.txt\" failed"
    [ "$BOOM_TRACE" ] && echo "cp zyre_classes.icl $_INSTALL_ROOT/bin/"
    cp zyre_classes.icl $_INSTALL_ROOT/bin/ || croak "\"cp zyre_classes.icl $_INSTALL_ROOT/bin/\" failed"
    [ "$BOOM_TRACE" ] && echo "chmod 0644 $_INSTALL_ROOT/bin/zyre_classes.icl"
    chmod 0644 $_INSTALL_ROOT/bin/zyre_classes.icl || croak "\"chmod 0644 $_INSTALL_ROOT/bin/zyre_classes.icl\" failed"
    mkdir -p $_INSTALL_ROOT/doc/OpenAMQ_Zyre/
    [ "$BOOM_TRACE" ] && echo "cp readme.txt $_INSTALL_ROOT/doc/OpenAMQ_Zyre/"
    cp readme.txt $_INSTALL_ROOT/doc/OpenAMQ_Zyre/ || croak "\"cp readme.txt $_INSTALL_ROOT/doc/OpenAMQ_Zyre/\" failed"
    [ "$BOOM_TRACE" ] && echo "chmod 0644 $_INSTALL_ROOT/doc/OpenAMQ_Zyre/readme.txt"
    chmod 0644 $_INSTALL_ROOT/doc/OpenAMQ_Zyre/readme.txt || croak "\"chmod 0644 $_INSTALL_ROOT/doc/OpenAMQ_Zyre/readme.txt\" failed"
    mkdir -p $_INSTALL_ROOT/include/
    [ "$BOOM_TRACE" ] && echo "cp zyre_config.h $_INSTALL_ROOT/include/"
    cp zyre_config.h $_INSTALL_ROOT/include/ || croak "\"cp zyre_config.h $_INSTALL_ROOT/include/\" failed"
    [ "$BOOM_TRACE" ] && echo "chmod 0644 $_INSTALL_ROOT/include/zyre_config.h"
    chmod 0644 $_INSTALL_ROOT/include/zyre_config.h || croak "\"chmod 0644 $_INSTALL_ROOT/include/zyre_config.h\" failed"
    mkdir -p $_INSTALL_ROOT/bin/
    [ "$BOOM_TRACE" ] && echo "cp zyre_backend_front.icl $_INSTALL_ROOT/bin/"
    cp zyre_backend_front.icl $_INSTALL_ROOT/bin/ || croak "\"cp zyre_backend_front.icl $_INSTALL_ROOT/bin/\" failed"
    [ "$BOOM_TRACE" ] && echo "chmod 0644 $_INSTALL_ROOT/bin/zyre_backend_front.icl"
    chmod 0644 $_INSTALL_ROOT/bin/zyre_backend_front.icl || croak "\"chmod 0644 $_INSTALL_ROOT/bin/zyre_backend_front.icl\" failed"
    [ "$BOOM_TRACE" ] && echo "cp zyre_backend_back.icl $_INSTALL_ROOT/bin/"
    cp zyre_backend_back.icl $_INSTALL_ROOT/bin/ || croak "\"cp zyre_backend_back.icl $_INSTALL_ROOT/bin/\" failed"
    [ "$BOOM_TRACE" ] && echo "chmod 0644 $_INSTALL_ROOT/bin/zyre_backend_back.icl"
    chmod 0644 $_INSTALL_ROOT/bin/zyre_backend_back.icl || croak "\"chmod 0644 $_INSTALL_ROOT/bin/zyre_backend_back.icl\" failed"
    mkdir -p $_INSTALL_ROOT/include/
    [ "$BOOM_TRACE" ] && echo "cp zyre_backend.h $_INSTALL_ROOT/include/"
    cp zyre_backend.h $_INSTALL_ROOT/include/ || croak "\"cp zyre_backend.h $_INSTALL_ROOT/include/\" failed"
    [ "$BOOM_TRACE" ] && echo "chmod 0644 $_INSTALL_ROOT/include/zyre_backend.h"
    chmod 0644 $_INSTALL_ROOT/include/zyre_backend.h || croak "\"chmod 0644 $_INSTALL_ROOT/include/zyre_backend.h\" failed"
    mkdir -p $_INSTALL_ROOT/bin/
    [ "$BOOM_TRACE" ] && echo "cp zyre_resource_front.icl $_INSTALL_ROOT/bin/"
    cp zyre_resource_front.icl $_INSTALL_ROOT/bin/ || croak "\"cp zyre_resource_front.icl $_INSTALL_ROOT/bin/\" failed"
    [ "$BOOM_TRACE" ] && echo "chmod 0644 $_INSTALL_ROOT/bin/zyre_resource_front.icl"
    chmod 0644 $_INSTALL_ROOT/bin/zyre_resource_front.icl || croak "\"chmod 0644 $_INSTALL_ROOT/bin/zyre_resource_front.icl\" failed"
    [ "$BOOM_TRACE" ] && echo "cp zyre_resource_back.icl $_INSTALL_ROOT/bin/"
    cp zyre_resource_back.icl $_INSTALL_ROOT/bin/ || croak "\"cp zyre_resource_back.icl $_INSTALL_ROOT/bin/\" failed"
    [ "$BOOM_TRACE" ] && echo "chmod 0644 $_INSTALL_ROOT/bin/zyre_resource_back.icl"
    chmod 0644 $_INSTALL_ROOT/bin/zyre_resource_back.icl || croak "\"chmod 0644 $_INSTALL_ROOT/bin/zyre_resource_back.icl\" failed"
    mkdir -p $_INSTALL_ROOT/include/
    [ "$BOOM_TRACE" ] && echo "cp zyre_resource.h $_INSTALL_ROOT/include/"
    cp zyre_resource.h $_INSTALL_ROOT/include/ || croak "\"cp zyre_resource.h $_INSTALL_ROOT/include/\" failed"
    [ "$BOOM_TRACE" ] && echo "chmod 0644 $_INSTALL_ROOT/include/zyre_resource.h"
    chmod 0644 $_INSTALL_ROOT/include/zyre_resource.h || croak "\"chmod 0644 $_INSTALL_ROOT/include/zyre_resource.h\" failed"
    [ "$BOOM_TRACE" ] && echo "cp zyre_domain.h $_INSTALL_ROOT/include/"
    cp zyre_domain.h $_INSTALL_ROOT/include/ || croak "\"cp zyre_domain.h $_INSTALL_ROOT/include/\" failed"
    [ "$BOOM_TRACE" ] && echo "chmod 0644 $_INSTALL_ROOT/include/zyre_domain.h"
    chmod 0644 $_INSTALL_ROOT/include/zyre_domain.h || croak "\"chmod 0644 $_INSTALL_ROOT/include/zyre_domain.h\" failed"
    [ "$BOOM_TRACE" ] && echo "cp zyre_feed.h $_INSTALL_ROOT/include/"
    cp zyre_feed.h $_INSTALL_ROOT/include/ || croak "\"cp zyre_feed.h $_INSTALL_ROOT/include/\" failed"
    [ "$BOOM_TRACE" ] && echo "chmod 0644 $_INSTALL_ROOT/include/zyre_feed.h"
    chmod 0644 $_INSTALL_ROOT/include/zyre_feed.h || croak "\"chmod 0644 $_INSTALL_ROOT/include/zyre_feed.h\" failed"
    [ "$BOOM_TRACE" ] && echo "cp zyre_pipe.h $_INSTALL_ROOT/include/"
    cp zyre_pipe.h $_INSTALL_ROOT/include/ || croak "\"cp zyre_pipe.h $_INSTALL_ROOT/include/\" failed"
    [ "$BOOM_TRACE" ] && echo "chmod 0644 $_INSTALL_ROOT/include/zyre_pipe.h"
    chmod 0644 $_INSTALL_ROOT/include/zyre_pipe.h || croak "\"chmod 0644 $_INSTALL_ROOT/include/zyre_pipe.h\" failed"
    [ "$BOOM_TRACE" ] && echo "cp zyre_join.h $_INSTALL_ROOT/include/"
    cp zyre_join.h $_INSTALL_ROOT/include/ || croak "\"cp zyre_join.h $_INSTALL_ROOT/include/\" failed"
    [ "$BOOM_TRACE" ] && echo "chmod 0644 $_INSTALL_ROOT/include/zyre_join.h"
    chmod 0644 $_INSTALL_ROOT/include/zyre_join.h || croak "\"chmod 0644 $_INSTALL_ROOT/include/zyre_join.h\" failed"
    [ "$BOOM_TRACE" ] && echo "cp zyre_message.h $_INSTALL_ROOT/include/"
    cp zyre_message.h $_INSTALL_ROOT/include/ || croak "\"cp zyre_message.h $_INSTALL_ROOT/include/\" failed"
    [ "$BOOM_TRACE" ] && echo "chmod 0644 $_INSTALL_ROOT/include/zyre_message.h"
    chmod 0644 $_INSTALL_ROOT/include/zyre_message.h || croak "\"chmod 0644 $_INSTALL_ROOT/include/zyre_message.h\" failed"
    [ "$BOOM_TRACE" ] && echo "cp zyre_content.h $_INSTALL_ROOT/include/"
    cp zyre_content.h $_INSTALL_ROOT/include/ || croak "\"cp zyre_content.h $_INSTALL_ROOT/include/\" failed"
    [ "$BOOM_TRACE" ] && echo "chmod 0644 $_INSTALL_ROOT/include/zyre_content.h"
    chmod 0644 $_INSTALL_ROOT/include/zyre_content.h || croak "\"chmod 0644 $_INSTALL_ROOT/include/zyre_content.h\" failed"
    [ "$BOOM_TRACE" ] && echo "cp zyre_restms.h $_INSTALL_ROOT/include/"
    cp zyre_restms.h $_INSTALL_ROOT/include/ || croak "\"cp zyre_restms.h $_INSTALL_ROOT/include/\" failed"
    [ "$BOOM_TRACE" ] && echo "chmod 0644 $_INSTALL_ROOT/include/zyre_restms.h"
    chmod 0644 $_INSTALL_ROOT/include/zyre_restms.h || croak "\"chmod 0644 $_INSTALL_ROOT/include/zyre_restms.h\" failed"
