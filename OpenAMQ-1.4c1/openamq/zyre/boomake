        croak "zyre_peer_agent.h is missing"
    fi
    if [ ! -f "zyre_peer_agent.c" ]; then
        croak "zyre_peer_agent.c is missing"
    fi
    if [ ! -f "zyre_classes.h" ]; then
        croak "zyre_classes.h is missing"
    fi
    if [ ! -f "zyre_classes.c" ]; then
        croak "zyre_classes.c is missing"
    fi
    if [ ! -f "zyre_static.icl" ]; then
        croak "zyre_static.icl is missing"
    fi
    if [ ! -f "zyre_static_test.c" ]; then
        croak "zyre_static_test.c is missing"
    fi
    if [ ! -f "zyre_static.h" ]; then
        croak "zyre_static.h is missing"
    fi
    if [ ! -f "zyre_static.c" ]; then
        croak "zyre_static.c is missing"
    fi
    if [ ! -f "stamp_generate" ]; then
        croak "stamp_generate is missing"
    fi
    if [ ! -f "boomakew.bat" ]; then
        croak "boomakew.bat is missing"
    fi
    if [ ! -f "configure" ]; then
        croak "configure is missing"
    fi
    if [ ! -f "Makefile.unix" ]; then
        croak "Makefile.unix is missing"
    fi
    if [ ! -f "boomake" ]; then
        croak "boomake is missing"
    fi
    if [ ! -f "configure.bat" ]; then
        croak "configure.bat is missing"
    fi
    if [ ! -f "Makefile.win32" ]; then
        croak "Makefile.win32 is missing"
    fi
    if [ ! -f "boomake.bat" ]; then
        croak "boomake.bat is missing"
    fi
    if [ ! -f "zyre.vcproj" ]; then
        croak "zyre.vcproj is missing"
    fi
    if [ ! -f "libamq_zyre.vcproj" ]; then
        croak "libamq_zyre.vcproj is missing"
    fi
    PUSHDIR=`pwd`
    cd webpages
    if [ ! -f "http_base.css" ]; then
        croak "http_base.css is missing"
    fi
    if [ ! -f "index.html" ]; then
        croak "index.html is missing"
    fi
    cd $PUSHDIR
}

# -------------------------   INSTALL DELIVERABLES   --------------------------

a_install_deliverables() {
:
    carp "Installing OpenAMQ Zyre into $_INSTALL_ROOT..."
    if [ ! -d "$_INSTALL_ROOT" ]; then
        if mkdir -p $_INSTALL_ROOT; then
            :
        else
            croak "Could not create directory \"$_INSTALL_ROOT\""
        fi
    fi
    mkdir -p $_INSTALL_ROOT/lib/
    [ "$BOOM_TRACE" ] && echo "cp libamq_zyre.a $_INSTALL_ROOT/lib/"
    cp libamq_zyre.a $_INSTALL_ROOT/lib/ || croak "\"cp libamq_zyre.a $_INSTALL_ROOT/lib/\" failed"
    [ "$BOOM_TRACE" ] && echo "chmod 0644 $_INSTALL_ROOT/lib/libamq_zyre.a"
    chmod 0644 $_INSTALL_ROOT/lib/libamq_zyre.a || croak "\"chmod 0644 $_INSTALL_ROOT/lib/libamq_zyre.a\" failed"
    mkdir -p $_INSTALL_ROOT/bin/
    [ "$BOOM_TRACE" ] && echo "cp zyre $_INSTALL_ROOT/bin/"
    cp zyre $_INSTALL_ROOT/bin/ || croak "\"cp zyre $_INSTALL_ROOT/bin/\" failed"
    [ "$BOOM_TRACE" ] && echo "chmod 0755 $_INSTALL_ROOT/bin/zyre"
    chmod 0755 $_INSTALL_ROOT/bin/zyre || croak "\"chmod 0755 $_INSTALL_ROOT/bin/zyre\" failed"
    [ "$BOOM_TRACE" ] && echo "cp zyre_domain.icl $_INSTALL_ROOT/bin/"
    cp zyre_domain.icl $_INSTALL_ROOT/bin/ || croak "\"cp zyre_domain.icl $_INSTALL_ROOT/bin/\" failed"
    [ "$BOOM_TRACE" ] && echo "chmod 0644 $_INSTALL_ROOT/bin/zyre_domain.icl"
    chmod 0644 $_INSTALL_ROOT/bin/zyre_domain.icl || croak "\"chmod 0644 $_INSTALL_ROOT/bin/zyre_domain.icl\" failed"
    [ "$BOOM_TRACE" ] && echo "cp zyre_feed.icl $_INSTALL_ROOT/bin/"
    cp zyre_feed.icl $_INSTALL_ROOT/bin/ || croak "\"cp zyre_feed.icl $_INSTALL_ROOT/bin/\" failed"
    [ "$BOOM_TRACE" ] && echo "chmod 0644 $_INSTALL_ROOT/bin/zyre_feed.icl"
    chmod 0644 $_INSTALL_ROOT/bin/zyre_feed.icl || croak "\"chmod 0644 $_INSTALL_ROOT/bin/zyre_feed.icl\" failed"
    [ "$BOOM_TRACE" ] && echo "cp zyre_pipe.icl $_INSTALL_ROOT/bin/"
    cp zyre_pipe.icl $_INSTALL_ROOT/bin/ || croak "\"cp zyre_pipe.icl $_INSTALL_ROOT/bin/\" failed"
    [ "$BOOM_TRACE" ] && echo "chmod 0644 $_INSTALL_ROOT/bin/zyre_pipe.icl"
    chmod 0644 $_INSTALL_ROOT/bin/zyre_pipe.icl || croak "\"chmod 0644 $_INSTALL_ROOT/bin/zyre_pipe.icl\" failed"
    [ "$BOOM_TRACE" ] && echo "cp zyre_join.icl $_INSTALL_ROOT/bin/"
    cp zyre_join.icl $_INSTALL_ROOT/bin/ || croak "\"cp zyre_join.icl $_INSTALL_ROOT/bin/\" failed"
    [ "$BOOM_TRACE" ] && echo "chmod 0644 $_INSTALL_ROOT/bin/zyre_join.icl"
    chmod 0644 $_INSTALL_ROOT/bin/zyre_join.icl || croak "\"chmod 0644 $_INSTALL_ROOT/bin/zyre_join.icl\" failed"
    [ "$BOOM_TRACE" ] && echo "cp zyre_message.icl $_INSTALL_ROOT/bin/"
    cp zyre_message.icl $_INSTALL_ROOT/bin/ || croak "\"cp zyre_message.icl $_INSTALL_ROOT/bin/\" failed"
    [ "$BOOM_TRACE" ] && echo "chmod 0644 $_INSTALL_ROOT/bin/zyre_message.icl"
    chmod 0644 $_INSTALL_ROOT/bin/zyre_message.icl || croak "\"chmod 0644 $_INSTALL_ROOT/bin/zyre_message.icl\" failed"
    [ "$BOOM_TRACE" ] && echo "cp zyre_content.icl $_INSTALL_ROOT/bin/"
    cp zyre_content.icl $_INSTALL_ROOT/bin/ || croak "\"cp zyre_content.icl $_INSTALL_ROOT/bin/\" failed"
    [ "$BOOM_TRACE" ] && echo "chmod 0644 $_INSTALL_ROOT/bin/zyre_content.icl"
    chmod 0644 $_INSTALL_ROOT/bin/zyre_content.icl || croak "\"chmod 0644 $_INSTALL_ROOT/bin/zyre_content.icl\" failed"
    [ "$BOOM_TRACE" ] && echo "cp zyre_amqp_feed.icl $_INSTALL_ROOT/bin/"
    cp zyre_amqp_feed.icl $_INSTALL_ROOT/bin/ || croak "\"cp zyre_amqp_feed.icl $_INSTALL_ROOT/bin/\" failed"
    [ "$BOOM_TRACE" ] && echo "chmod 0644 $_INSTALL_ROOT/bin/zyre_amqp_feed.icl"
    chmod 0644 $_INSTALL_ROOT/bin/zyre_amqp_feed.icl || croak "\"chmod 0644 $_INSTALL_ROOT/bin/zyre_amqp_feed.icl\" failed"
    [ "$BOOM_TRACE" ] && echo "cp zyre_amqp_pipe.icl $_INSTALL_ROOT/bin/"
    cp zyre_amqp_pipe.icl $_INSTALL_ROOT/bin/ || croak "\"cp zyre_amqp_pipe.icl $_INSTALL_ROOT/bin/\" failed"
    [ "$BOOM_TRACE" ] && echo "chmod 0644 $_INSTALL_ROOT/bin/zyre_amqp_pipe.icl"
    chmod 0644 $_INSTALL_ROOT/bin/zyre_amqp_pipe.icl || croak "\"chmod 0644 $_INSTALL_ROOT/bin/zyre_amqp_pipe.icl\" failed"
    [ "$BOOM_TRACE" ] && echo "cp zyre_amqp_join.icl $_INSTALL_ROOT/bin/"
    cp zyre_amqp_join.icl $_INSTALL_ROOT/bin/ || croak "\"cp zyre_amqp_join.icl $_INSTALL_ROOT/bin/\" failed"
