    chmod 0644 $_INSTALL_ROOT/include/zyre_amqp_join.h || croak "\"chmod 0644 $_INSTALL_ROOT/include/zyre_amqp_join.h\" failed"
    [ "$BOOM_TRACE" ] && echo "cp zyre_backend_amqp.h $_INSTALL_ROOT/include/"
    cp zyre_backend_amqp.h $_INSTALL_ROOT/include/ || croak "\"cp zyre_backend_amqp.h $_INSTALL_ROOT/include/\" failed"
    [ "$BOOM_TRACE" ] && echo "chmod 0644 $_INSTALL_ROOT/include/zyre_backend_amqp.h"
    chmod 0644 $_INSTALL_ROOT/include/zyre_backend_amqp.h || croak "\"chmod 0644 $_INSTALL_ROOT/include/zyre_backend_amqp.h\" failed"
    [ "$BOOM_TRACE" ] && echo "cp zyre_digest_amqp.h $_INSTALL_ROOT/include/"
    cp zyre_digest_amqp.h $_INSTALL_ROOT/include/ || croak "\"cp zyre_digest_amqp.h $_INSTALL_ROOT/include/\" failed"
    [ "$BOOM_TRACE" ] && echo "chmod 0644 $_INSTALL_ROOT/include/zyre_digest_amqp.h"
    chmod 0644 $_INSTALL_ROOT/include/zyre_digest_amqp.h || croak "\"chmod 0644 $_INSTALL_ROOT/include/zyre_digest_amqp.h\" failed"
    [ "$BOOM_TRACE" ] && echo "cp zyre_peer_method.h $_INSTALL_ROOT/include/"
    cp zyre_peer_method.h $_INSTALL_ROOT/include/ || croak "\"cp zyre_peer_method.h $_INSTALL_ROOT/include/\" failed"
    [ "$BOOM_TRACE" ] && echo "chmod 0644 $_INSTALL_ROOT/include/zyre_peer_method.h"
    chmod 0644 $_INSTALL_ROOT/include/zyre_peer_method.h || croak "\"chmod 0644 $_INSTALL_ROOT/include/zyre_peer_method.h\" failed"
    [ "$BOOM_TRACE" ] && echo "cp zyre_peer_config.h $_INSTALL_ROOT/include/"
    cp zyre_peer_config.h $_INSTALL_ROOT/include/ || croak "\"cp zyre_peer_config.h $_INSTALL_ROOT/include/\" failed"
    [ "$BOOM_TRACE" ] && echo "chmod 0644 $_INSTALL_ROOT/include/zyre_peer_config.h"
    chmod 0644 $_INSTALL_ROOT/include/zyre_peer_config.h || croak "\"chmod 0644 $_INSTALL_ROOT/include/zyre_peer_config.h\" failed"
    [ "$BOOM_TRACE" ] && echo "cp zyre_classes.h $_INSTALL_ROOT/include/"
    cp zyre_classes.h $_INSTALL_ROOT/include/ || croak "\"cp zyre_classes.h $_INSTALL_ROOT/include/\" failed"
    [ "$BOOM_TRACE" ] && echo "chmod 0644 $_INSTALL_ROOT/include/zyre_classes.h"
    chmod 0644 $_INSTALL_ROOT/include/zyre_classes.h || croak "\"chmod 0644 $_INSTALL_ROOT/include/zyre_classes.h\" failed"
    [ "$BOOM_TRACE" ] && echo "cp zyre_static.h $_INSTALL_ROOT/include/"
    cp zyre_static.h $_INSTALL_ROOT/include/ || croak "\"cp zyre_static.h $_INSTALL_ROOT/include/\" failed"
    [ "$BOOM_TRACE" ] && echo "chmod 0644 $_INSTALL_ROOT/include/zyre_static.h"
    chmod 0644 $_INSTALL_ROOT/include/zyre_static.h || croak "\"chmod 0644 $_INSTALL_ROOT/include/zyre_static.h\" failed"
    mkdir -p $_INSTALL_ROOT/bin/
    [ "$BOOM_TRACE" ] && echo "cp webpages/http_base.css $_INSTALL_ROOT/bin/"
    cp webpages/http_base.css $_INSTALL_ROOT/bin/ || croak "\"cp webpages/http_base.css $_INSTALL_ROOT/bin/\" failed"
    [ "$BOOM_TRACE" ] && echo "chmod 0644 $_INSTALL_ROOT/bin/http_base.css"
    chmod 0644 $_INSTALL_ROOT/bin/http_base.css || croak "\"chmod 0644 $_INSTALL_ROOT/bin/http_base.css\" failed"
    [ "$BOOM_TRACE" ] && echo "cp webpages/index.html $_INSTALL_ROOT/bin/"
    cp webpages/index.html $_INSTALL_ROOT/bin/ || croak "\"cp webpages/index.html $_INSTALL_ROOT/bin/\" failed"
    [ "$BOOM_TRACE" ] && echo "chmod 0644 $_INSTALL_ROOT/bin/index.html"
    chmod 0644 $_INSTALL_ROOT/bin/index.html || croak "\"chmod 0644 $_INSTALL_ROOT/bin/index.html\" failed"
    if [ -f ./installer ]; then
    [ "$BOOM_TRACE" ] && echo "sh ./installer"
    sh ./installer || croak "\"sh ./installer\" failed"
    fi
}

# ----------------------   PRODUCE ALL GENERATED FILES   ----------------------

a_produce_all_generated_files() {
:
    carp "Generating files..."
    [ "$BOOM_TRACE" ] && echo "gsl -q -quiet:1 zyre_config.opf"
    gsl -q -quiet:1 zyre_config.opf || croak "\"gsl -q -quiet:1 zyre_config.opf\" failed"
    [ "$BOOM_TRACE" ] && echo "gsl -q -quiet:1 zyre_config.icl"
    gsl -q -quiet:1 zyre_config.icl || croak "\"gsl -q -quiet:1 zyre_config.icl\" failed"
    [ "$BOOM_TRACE" ] && echo "gsl -q -quiet:1 zyre_backend.icl"
    gsl -q -quiet:1 zyre_backend.icl || croak "\"gsl -q -quiet:1 zyre_backend.icl\" failed"
    [ "$BOOM_TRACE" ] && echo "gsl -q -quiet:1 zyre_resource.icl"
    gsl -q -quiet:1 zyre_resource.icl || croak "\"gsl -q -quiet:1 zyre_resource.icl\" failed"
    [ "$BOOM_TRACE" ] && echo "gsl -q -quiet:1 zyre_domain.icl"
    gsl -q -quiet:1 zyre_domain.icl || croak "\"gsl -q -quiet:1 zyre_domain.icl\" failed"
    [ "$BOOM_TRACE" ] && echo "gsl -q -quiet:1 zyre_feed.icl"
    gsl -q -quiet:1 zyre_feed.icl || croak "\"gsl -q -quiet:1 zyre_feed.icl\" failed"
    [ "$BOOM_TRACE" ] && echo "gsl -q -quiet:1 zyre_pipe.icl"
    gsl -q -quiet:1 zyre_pipe.icl || croak "\"gsl -q -quiet:1 zyre_pipe.icl\" failed"
    [ "$BOOM_TRACE" ] && echo "gsl -q -quiet:1 zyre_join.icl"
    gsl -q -quiet:1 zyre_join.icl || croak "\"gsl -q -quiet:1 zyre_join.icl\" failed"
    [ "$BOOM_TRACE" ] && echo "gsl -q -quiet:1 zyre_message.icl"
    gsl -q -quiet:1 zyre_message.icl || croak "\"gsl -q -quiet:1 zyre_message.icl\" failed"
    [ "$BOOM_TRACE" ] && echo "gsl -q -quiet:1 zyre_content.icl"
    gsl -q -quiet:1 zyre_content.icl || croak "\"gsl -q -quiet:1 zyre_content.icl\" failed"
    [ "$BOOM_TRACE" ] && echo "gsl -q -quiet:1 zyre_restms.icl"
    gsl -q -quiet:1 zyre_restms.icl || croak "\"gsl -q -quiet:1 zyre_restms.icl\" failed"
    [ "$BOOM_TRACE" ] && echo "gsl -q -quiet:1 zyre_amqp_feed.icl"
    gsl -q -quiet:1 zyre_amqp_feed.icl || croak "\"gsl -q -quiet:1 zyre_amqp_feed.icl\" failed"
    [ "$BOOM_TRACE" ] && echo "gsl -q -quiet:1 zyre_amqp_pipe.icl"
    gsl -q -quiet:1 zyre_amqp_pipe.icl || croak "\"gsl -q -quiet:1 zyre_amqp_pipe.icl\" failed"
    [ "$BOOM_TRACE" ] && echo "gsl -q -quiet:1 zyre_amqp_join.icl"
    gsl -q -quiet:1 zyre_amqp_join.icl || croak "\"gsl -q -quiet:1 zyre_amqp_join.icl\" failed"
    [ "$BOOM_TRACE" ] && echo "gsl -q -quiet:1 zyre_backend_amqp.icl"
    gsl -q -quiet:1 zyre_backend_amqp.icl || croak "\"gsl -q -quiet:1 zyre_backend_amqp.icl\" failed"
    [ "$BOOM_TRACE" ] && echo "gsl -q -quiet:1 zyre_digest_amqp.icl"
    gsl -q -quiet:1 zyre_digest_amqp.icl || croak "\"gsl -q -quiet:1 zyre_digest_amqp.icl\" failed"
    [ "$BOOM_TRACE" ] && echo "gsl -q -quiet:1 zyre_peer.asl"
    gsl -q -quiet:1 zyre_peer.asl || croak "\"gsl -q -quiet:1 zyre_peer.asl\" failed"
    [ "$BOOM_TRACE" ] && echo "gsl -q -quiet:1 zyre_peer_method.icl"
    gsl -q -quiet:1 zyre_peer_method.icl || croak "\"gsl -q -quiet:1 zyre_peer_method.icl\" failed"
    [ "$BOOM_TRACE" ] && echo "gsl -q -quiet:1 zyre_peer_config.opf"
    gsl -q -quiet:1 zyre_peer_config.opf || croak "\"gsl -q -quiet:1 zyre_peer_config.opf\" failed"
    [ "$BOOM_TRACE" ] && echo "gsl -q -quiet:1 zyre_peer_config.icl"
    gsl -q -quiet:1 zyre_peer_config.icl || croak "\"gsl -q -quiet:1 zyre_peer_config.icl\" failed"
    [ "$BOOM_TRACE" ] && echo "gsl -q -quiet:1 zyre_peer_agent.smt"
    gsl -q -quiet:1 zyre_peer_agent.smt || croak "\"gsl -q -quiet:1 zyre_peer_agent.smt\" failed"
    [ "$BOOM_TRACE" ] && echo "gsl -q -quiet:1 zyre_classes.icl"
    gsl -q -quiet:1 zyre_classes.icl || croak "\"gsl -q -quiet:1 zyre_classes.icl\" failed"
    [ "$BOOM_TRACE" ] && echo "perl -S ipr_resource.pl -@ zyre_static.rc"
    perl -S ipr_resource.pl -@ zyre_static.rc || croak "\"perl -S ipr_resource.pl -@ zyre_static.rc\" failed"
    [ "$BOOM_TRACE" ] && echo "gsl -q -quiet:1 zyre_restms_agent.smt"
    gsl -q -quiet:1 zyre_restms_agent.smt || croak "\"gsl -q -quiet:1 zyre_restms_agent.smt\" failed"
    [ "$BOOM_TRACE" ] && echo "gsl -q -quiet:1 zyre_backend_amqp_agent.smt"
    gsl -q -quiet:1 zyre_backend_amqp_agent.smt || croak "\"gsl -q -quiet:1 zyre_backend_amqp_agent.smt\" failed"
    [ "$BOOM_TRACE" ] && echo "gsl -q -quiet:1 zyre_digest_amqp_agent.smt"
    gsl -q -quiet:1 zyre_digest_amqp_agent.smt || croak "\"gsl -q -quiet:1 zyre_digest_amqp_agent.smt\" failed"
    [ "$BOOM_TRACE" ] && echo "gsl -q -quiet:1 zyre_static.icl"
    gsl -q -quiet:1 zyre_static.icl || croak "\"gsl -q -quiet:1 zyre_static.icl\" failed"
    PUSHDIR=`pwd`
    cd webpages
    cd $PUSHDIR
    echo "Source files successfully generated">stamp_generate
}

# ------------------------   PRODUCE GENERATED FILES   ------------------------

a_produce_generated_files() {
:
    carp "Generating files..."
    [ "$BOOM_TRACE" ] && echo "gsl -q -quiet:1 zyre_config.opf"
    gsl -q -quiet:1 zyre_config.opf || croak "\"gsl -q -quiet:1 zyre_config.opf\" failed"
    [ "$BOOM_TRACE" ] && echo "gsl -q -quiet:1 zyre_config.icl"
    gsl -q -quiet:1 zyre_config.icl || croak "\"gsl -q -quiet:1 zyre_config.icl\" failed"
    [ "$BOOM_TRACE" ] && echo "gsl -q -quiet:1 zyre_backend.icl"
    gsl -q -quiet:1 zyre_backend.icl || croak "\"gsl -q -quiet:1 zyre_backend.icl\" failed"
    [ "$BOOM_TRACE" ] && echo "gsl -q -quiet:1 zyre_resource.icl"
