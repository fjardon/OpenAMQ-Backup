    c $MY_COPTS -L amq_failover_test || croak "\"c $MY_COPTS -L amq_failover_test\" failed"
    [ "$BOOM_TRACE" ] && echo "c $MY_COPTS -L amq_lease_test"
    c $MY_COPTS -L amq_lease_test || croak "\"c $MY_COPTS -L amq_lease_test\" failed"
    [ "$BOOM_TRACE" ] && echo "c $MY_COPTS -L amq_lease_table_test"
    c $MY_COPTS -L amq_lease_table_test || croak "\"c $MY_COPTS -L amq_lease_table_test\" failed"
    [ "$BOOM_TRACE" ] && echo "c $MY_COPTS -L amq_console_test"
    c $MY_COPTS -L amq_console_test || croak "\"c $MY_COPTS -L amq_console_test\" failed"
    [ "$BOOM_TRACE" ] && echo "c $MY_COPTS -L amq_console_class_test"
    c $MY_COPTS -L amq_console_class_test || croak "\"c $MY_COPTS -L amq_console_class_test\" failed"
