    fi
    if [ ! -f "amq_connection.h" ]; then
        croak "amq_connection.h is missing"
    fi
    if [ ! -f "amq_connection_by_broker.h" ]; then
        croak "amq_connection_by_broker.h is missing"
    fi
    if [ ! -f "amq_consumer.h" ]; then
        croak "amq_consumer.h is missing"
    fi
    if [ ! -f "amq_consumer_table.h" ]; then
        croak "amq_consumer_table.h is missing"
    fi
    if [ ! -f "amq_consumer_by_queue.h" ]; then
        croak "amq_consumer_by_queue.h is missing"
    fi
    if [ ! -f "amq_consumer_by_channel.h" ]; then
        croak "amq_consumer_by_channel.h is missing"
    fi
    if [ ! -f "amq_consumer_basic.h" ]; then
        croak "amq_consumer_basic.h is missing"
    fi
    if [ ! -f "amq_queue_connection_agent.smt" ]; then
        croak "amq_queue_connection_agent.smt is missing"
    fi
    if [ ! -f "amq_queue_connection_agent.h" ]; then
        croak "amq_queue_connection_agent.h is missing"
    fi
    if [ ! -f "amq_queue_connection.h" ]; then
        croak "amq_queue_connection.h is missing"
    fi
    if [ ! -f "amq_queue_binding_agent.smt" ]; then
        croak "amq_queue_binding_agent.smt is missing"
    fi
    if [ ! -f "amq_queue_binding_agent.h" ]; then
        croak "amq_queue_binding_agent.h is missing"
    fi
    if [ ! -f "amq_queue_binding.h" ]; then
        croak "amq_queue_binding.h is missing"
    fi
    if [ ! -f "amq_connection_queue_agent.smt" ]; then
        croak "amq_connection_queue_agent.smt is missing"
    fi
    if [ ! -f "amq_connection_queue_agent.h" ]; then
        croak "amq_connection_queue_agent.h is missing"
    fi
    if [ ! -f "amq_connection_queue.h" ]; then
        croak "amq_connection_queue.h is missing"
    fi
    if [ ! -f "amq_resource.h" ]; then
        croak "amq_resource.h is missing"
    fi
    if [ ! -f "amq_server_method.h" ]; then
        croak "amq_server_method.h is missing"
    fi
    if [ ! -f "amq_federation_agent.smt" ]; then
        croak "amq_federation_agent.smt is missing"
    fi
    if [ ! -f "amq_federation_agent.h" ]; then
        croak "amq_federation_agent.h is missing"
    fi
    if [ ! -f "amq_federation.h" ]; then
        croak "amq_federation.h is missing"
    fi
    if [ ! -f "amq_failover_agent.smt" ]; then
        croak "amq_failover_agent.smt is missing"
    fi
    if [ ! -f "amq_failover_agent.h" ]; then
        croak "amq_failover_agent.h is missing"
    fi
    if [ ! -f "amq_failover.h" ]; then
        croak "amq_failover.h is missing"
    fi
    if [ ! -f "amq_lease.h" ]; then
        croak "amq_lease.h is missing"
    fi
    if [ ! -f "amq_lease_table.h" ]; then
        croak "amq_lease_table.h is missing"
