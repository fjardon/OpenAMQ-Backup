    cp amq_peering_agent.h $_INSTALL_ROOT/include/ || croak "\"cp amq_peering_agent.h $_INSTALL_ROOT/include/\" failed"
    [ "$BOOM_TRACE" ] && echo "chmod 0644 $_INSTALL_ROOT/include/amq_peering_agent.h"
    chmod 0644 $_INSTALL_ROOT/include/amq_peering_agent.h || croak "\"chmod 0644 $_INSTALL_ROOT/include/amq_peering_agent.h\" failed"
