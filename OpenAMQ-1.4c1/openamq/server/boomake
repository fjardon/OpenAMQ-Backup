    chmod 0644 $_INSTALL_ROOT/include/amq_broker_agent.h || croak "\"chmod 0644 $_INSTALL_ROOT/include/amq_broker_agent.h\" failed"
    [ "$BOOM_TRACE" ] && echo "cp amq_broker.h $_INSTALL_ROOT/include/"
    cp amq_broker.h $_INSTALL_ROOT/include/ || croak "\"cp amq_broker.h $_INSTALL_ROOT/include/\" failed"
