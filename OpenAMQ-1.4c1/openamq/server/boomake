# -----------------------   REMOVE GENERATED BINARIES   -----------------------

a_remove_generated_binaries() {
:
    rm -f libamq_server.a
    rm -f amq_server
    rm -f amq_server.o
    rm -f amq_server
    rm -f amq_server_config_test_test.o
