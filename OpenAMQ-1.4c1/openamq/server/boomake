    c $MY_COPTS amq_server_connection_table_test || croak "\"c $MY_COPTS amq_server_connection_table_test\" failed"
    [ "$BOOM_TRACE" ] && echo "c $MY_COPTS amq_server_connection_table"
    c $MY_COPTS amq_server_connection_table || croak "\"c $MY_COPTS amq_server_connection_table\" failed"
