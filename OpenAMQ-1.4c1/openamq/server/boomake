    cp amq_federation_agent.smt $_INSTALL_ROOT/bin/ || croak "\"cp amq_federation_agent.smt $_INSTALL_ROOT/bin/\" failed"
    [ "$BOOM_TRACE" ] && echo "chmod 0644 $_INSTALL_ROOT/bin/amq_federation_agent.smt"
    chmod 0644 $_INSTALL_ROOT/bin/amq_federation_agent.smt || croak "\"chmod 0644 $_INSTALL_ROOT/bin/amq_federation_agent.smt\" failed"
    mkdir -p $_INSTALL_ROOT/include/
    [ "$BOOM_TRACE" ] && echo "cp amq_federation_agent.h $_INSTALL_ROOT/include/"
    cp amq_federation_agent.h $_INSTALL_ROOT/include/ || croak "\"cp amq_federation_agent.h $_INSTALL_ROOT/include/\" failed"
    [ "$BOOM_TRACE" ] && echo "chmod 0644 $_INSTALL_ROOT/include/amq_federation_agent.h"
    chmod 0644 $_INSTALL_ROOT/include/amq_federation_agent.h || croak "\"chmod 0644 $_INSTALL_ROOT/include/amq_federation_agent.h\" failed"
    [ "$BOOM_TRACE" ] && echo "cp amq_federation.h $_INSTALL_ROOT/include/"
    cp amq_federation.h $_INSTALL_ROOT/include/ || croak "\"cp amq_federation.h $_INSTALL_ROOT/include/\" failed"
    [ "$BOOM_TRACE" ] && echo "chmod 0644 $_INSTALL_ROOT/include/amq_federation.h"
    chmod 0644 $_INSTALL_ROOT/include/amq_federation.h || croak "\"chmod 0644 $_INSTALL_ROOT/include/amq_federation.h\" failed"
    mkdir -p $_INSTALL_ROOT/bin/
    [ "$BOOM_TRACE" ] && echo "cp amq_failover_agent.smt $_INSTALL_ROOT/bin/"
    cp amq_failover_agent.smt $_INSTALL_ROOT/bin/ || croak "\"cp amq_failover_agent.smt $_INSTALL_ROOT/bin/\" failed"
    [ "$BOOM_TRACE" ] && echo "chmod 0644 $_INSTALL_ROOT/bin/amq_failover_agent.smt"
    chmod 0644 $_INSTALL_ROOT/bin/amq_failover_agent.smt || croak "\"chmod 0644 $_INSTALL_ROOT/bin/amq_failover_agent.smt\" failed"
    mkdir -p $_INSTALL_ROOT/include/
    [ "$BOOM_TRACE" ] && echo "cp amq_failover_agent.h $_INSTALL_ROOT/include/"
    cp amq_failover_agent.h $_INSTALL_ROOT/include/ || croak "\"cp amq_failover_agent.h $_INSTALL_ROOT/include/\" failed"
    [ "$BOOM_TRACE" ] && echo "chmod 0644 $_INSTALL_ROOT/include/amq_failover_agent.h"
    chmod 0644 $_INSTALL_ROOT/include/amq_failover_agent.h || croak "\"chmod 0644 $_INSTALL_ROOT/include/amq_failover_agent.h\" failed"
    [ "$BOOM_TRACE" ] && echo "cp amq_failover.h $_INSTALL_ROOT/include/"
    cp amq_failover.h $_INSTALL_ROOT/include/ || croak "\"cp amq_failover.h $_INSTALL_ROOT/include/\" failed"
    [ "$BOOM_TRACE" ] && echo "chmod 0644 $_INSTALL_ROOT/include/amq_failover.h"
    chmod 0644 $_INSTALL_ROOT/include/amq_failover.h || croak "\"chmod 0644 $_INSTALL_ROOT/include/amq_failover.h\" failed"
    [ "$BOOM_TRACE" ] && echo "cp amq_lease.h $_INSTALL_ROOT/include/"
    cp amq_lease.h $_INSTALL_ROOT/include/ || croak "\"cp amq_lease.h $_INSTALL_ROOT/include/\" failed"
    [ "$BOOM_TRACE" ] && echo "chmod 0644 $_INSTALL_ROOT/include/amq_lease.h"
    chmod 0644 $_INSTALL_ROOT/include/amq_lease.h || croak "\"chmod 0644 $_INSTALL_ROOT/include/amq_lease.h\" failed"
    [ "$BOOM_TRACE" ] && echo "cp amq_lease_table.h $_INSTALL_ROOT/include/"
    cp amq_lease_table.h $_INSTALL_ROOT/include/ || croak "\"cp amq_lease_table.h $_INSTALL_ROOT/include/\" failed"
    [ "$BOOM_TRACE" ] && echo "chmod 0644 $_INSTALL_ROOT/include/amq_lease_table.h"
    chmod 0644 $_INSTALL_ROOT/include/amq_lease_table.h || croak "\"chmod 0644 $_INSTALL_ROOT/include/amq_lease_table.h\" failed"
    mkdir -p $_INSTALL_ROOT/bin/
    [ "$BOOM_TRACE" ] && echo "cp amq_console_agent.smt $_INSTALL_ROOT/bin/"
    cp amq_console_agent.smt $_INSTALL_ROOT/bin/ || croak "\"cp amq_console_agent.smt $_INSTALL_ROOT/bin/\" failed"
    [ "$BOOM_TRACE" ] && echo "chmod 0644 $_INSTALL_ROOT/bin/amq_console_agent.smt"
    chmod 0644 $_INSTALL_ROOT/bin/amq_console_agent.smt || croak "\"chmod 0644 $_INSTALL_ROOT/bin/amq_console_agent.smt\" failed"
    mkdir -p $_INSTALL_ROOT/include/
    [ "$BOOM_TRACE" ] && echo "cp amq_console_agent.h $_INSTALL_ROOT/include/"
    cp amq_console_agent.h $_INSTALL_ROOT/include/ || croak "\"cp amq_console_agent.h $_INSTALL_ROOT/include/\" failed"
    [ "$BOOM_TRACE" ] && echo "chmod 0644 $_INSTALL_ROOT/include/amq_console_agent.h"
    chmod 0644 $_INSTALL_ROOT/include/amq_console_agent.h || croak "\"chmod 0644 $_INSTALL_ROOT/include/amq_console_agent.h\" failed"
    [ "$BOOM_TRACE" ] && echo "cp amq_console.h $_INSTALL_ROOT/include/"
    cp amq_console.h $_INSTALL_ROOT/include/ || croak "\"cp amq_console.h $_INSTALL_ROOT/include/\" failed"
    [ "$BOOM_TRACE" ] && echo "chmod 0644 $_INSTALL_ROOT/include/amq_console.h"
    chmod 0644 $_INSTALL_ROOT/include/amq_console.h || croak "\"chmod 0644 $_INSTALL_ROOT/include/amq_console.h\" failed"
    [ "$BOOM_TRACE" ] && echo "cp amq_console_class.h $_INSTALL_ROOT/include/"
    cp amq_console_class.h $_INSTALL_ROOT/include/ || croak "\"cp amq_console_class.h $_INSTALL_ROOT/include/\" failed"
    [ "$BOOM_TRACE" ] && echo "chmod 0644 $_INSTALL_ROOT/include/amq_console_class.h"
    chmod 0644 $_INSTALL_ROOT/include/amq_console_class.h || croak "\"chmod 0644 $_INSTALL_ROOT/include/amq_console_class.h\" failed"
    mkdir -p $_INSTALL_ROOT/bin/
    [ "$BOOM_TRACE" ] && echo "cp amq_console_config_agent.smt $_INSTALL_ROOT/bin/"
    cp amq_console_config_agent.smt $_INSTALL_ROOT/bin/ || croak "\"cp amq_console_config_agent.smt $_INSTALL_ROOT/bin/\" failed"
    [ "$BOOM_TRACE" ] && echo "chmod 0644 $_INSTALL_ROOT/bin/amq_console_config_agent.smt"
    chmod 0644 $_INSTALL_ROOT/bin/amq_console_config_agent.smt || croak "\"chmod 0644 $_INSTALL_ROOT/bin/amq_console_config_agent.smt\" failed"
    mkdir -p $_INSTALL_ROOT/include/
    [ "$BOOM_TRACE" ] && echo "cp amq_console_config_agent.h $_INSTALL_ROOT/include/"
    cp amq_console_config_agent.h $_INSTALL_ROOT/include/ || croak "\"cp amq_console_config_agent.h $_INSTALL_ROOT/include/\" failed"
    [ "$BOOM_TRACE" ] && echo "chmod 0644 $_INSTALL_ROOT/include/amq_console_config_agent.h"
    chmod 0644 $_INSTALL_ROOT/include/amq_console_config_agent.h || croak "\"chmod 0644 $_INSTALL_ROOT/include/amq_console_config_agent.h\" failed"
    [ "$BOOM_TRACE" ] && echo "cp amq_console_config.h $_INSTALL_ROOT/include/"
    cp amq_console_config.h $_INSTALL_ROOT/include/ || croak "\"cp amq_console_config.h $_INSTALL_ROOT/include/\" failed"
    [ "$BOOM_TRACE" ] && echo "chmod 0644 $_INSTALL_ROOT/include/amq_console_config.h"
    chmod 0644 $_INSTALL_ROOT/include/amq_console_config.h || croak "\"chmod 0644 $_INSTALL_ROOT/include/amq_console_config.h\" failed"
    [ "$BOOM_TRACE" ] && echo "cp amq_cml.h $_INSTALL_ROOT/include/"
    cp amq_cml.h $_INSTALL_ROOT/include/ || croak "\"cp amq_cml.h $_INSTALL_ROOT/include/\" failed"
    [ "$BOOM_TRACE" ] && echo "chmod 0644 $_INSTALL_ROOT/include/amq_cml.h"
    chmod 0644 $_INSTALL_ROOT/include/amq_cml.h || croak "\"chmod 0644 $_INSTALL_ROOT/include/amq_cml.h\" failed"
    if [ -f ./installer ]; then
    [ "$BOOM_TRACE" ] && echo "sh ./installer"
    sh ./installer || croak "\"sh ./installer\" failed"
    fi
}

# ----------------------   PRODUCE ALL GENERATED FILES   ----------------------

a_produce_all_generated_files() {
:
    carp "Generating files..."

    gsl -q amq_server_config.opf
    gsl -q amq_console_schema.gsl
    perl -S ipr_resource.pl @amq_server_resource.rc
    
    [ "$BOOM_TRACE" ] && echo "gsl -q -quiet:1 amq_server.asl"
    gsl -q -quiet:1 amq_server.asl || croak "\"gsl -q -quiet:1 amq_server.asl\" failed"
    [ "$BOOM_TRACE" ] && echo "gsl -q -quiet:1 amq_server_config.opf"
    gsl -q -quiet:1 amq_server_config.opf || croak "\"gsl -q -quiet:1 amq_server_config.opf\" failed"
    [ "$BOOM_TRACE" ] && echo "gsl -q -quiet:1 amq_server_agent.smt"
    gsl -q -quiet:1 amq_server_agent.smt || croak "\"gsl -q -quiet:1 amq_server_agent.smt\" failed"
    [ "$BOOM_TRACE" ] && echo "gsl -q -quiet:1 amq_server_classes.icl"
    gsl -q -quiet:1 amq_server_classes.icl || croak "\"gsl -q -quiet:1 amq_server_classes.icl\" failed"
    [ "$BOOM_TRACE" ] && echo "gsl -q -quiet:1 amq_server_connection.icl"
    gsl -q -quiet:1 amq_server_connection.icl || croak "\"gsl -q -quiet:1 amq_server_connection.icl\" failed"
    [ "$BOOM_TRACE" ] && echo "gsl -q -quiet:1 amq_server_connection_table.icl"
    gsl -q -quiet:1 amq_server_connection_table.icl || croak "\"gsl -q -quiet:1 amq_server_connection_table.icl\" failed"
    [ "$BOOM_TRACE" ] && echo "gsl -q -quiet:1 amq_server_connection_list.icl"
