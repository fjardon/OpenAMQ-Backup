# -------------------------   INSTALL DELIVERABLES   --------------------------

a_install_deliverables() {
:
    carp "Installing OpenAMQ Server into $_INSTALL_ROOT..."
    if [ ! -d "$_INSTALL_ROOT" ]; then
        if mkdir -p $_INSTALL_ROOT; then
            :
        else
            croak "Could not create directory \"$_INSTALL_ROOT\""
        fi
    fi
    mkdir -p $_INSTALL_ROOT/lib/
    [ "$BOOM_TRACE" ] && echo "cp libamq_server.a $_INSTALL_ROOT/lib/"
    cp libamq_server.a $_INSTALL_ROOT/lib/ || croak "\"cp libamq_server.a $_INSTALL_ROOT/lib/\" failed"
    [ "$BOOM_TRACE" ] && echo "chmod 0644 $_INSTALL_ROOT/lib/libamq_server.a"
    chmod 0644 $_INSTALL_ROOT/lib/libamq_server.a || croak "\"chmod 0644 $_INSTALL_ROOT/lib/libamq_server.a\" failed"
    mkdir -p $_INSTALL_ROOT/bin/
    [ "$BOOM_TRACE" ] && echo "cp amq_server_config.opf $_INSTALL_ROOT/bin/"
    cp amq_server_config.opf $_INSTALL_ROOT/bin/ || croak "\"cp amq_server_config.opf $_INSTALL_ROOT/bin/\" failed"
    [ "$BOOM_TRACE" ] && echo "chmod 0644 $_INSTALL_ROOT/bin/amq_server_config.opf"
    chmod 0644 $_INSTALL_ROOT/bin/amq_server_config.opf || croak "\"chmod 0644 $_INSTALL_ROOT/bin/amq_server_config.opf\" failed"
    [ "$BOOM_TRACE" ] && echo "cp amq_server_agent.smt $_INSTALL_ROOT/bin/"
    cp amq_server_agent.smt $_INSTALL_ROOT/bin/ || croak "\"cp amq_server_agent.smt $_INSTALL_ROOT/bin/\" failed"
    [ "$BOOM_TRACE" ] && echo "chmod 0644 $_INSTALL_ROOT/bin/amq_server_agent.smt"
    chmod 0644 $_INSTALL_ROOT/bin/amq_server_agent.smt || croak "\"chmod 0644 $_INSTALL_ROOT/bin/amq_server_agent.smt\" failed"
    [ "$BOOM_TRACE" ] && echo "cp amq_server_classes.icl $_INSTALL_ROOT/bin/"
    cp amq_server_classes.icl $_INSTALL_ROOT/bin/ || croak "\"cp amq_server_classes.icl $_INSTALL_ROOT/bin/\" failed"
    [ "$BOOM_TRACE" ] && echo "chmod 0644 $_INSTALL_ROOT/bin/amq_server_classes.icl"
    chmod 0644 $_INSTALL_ROOT/bin/amq_server_classes.icl || croak "\"chmod 0644 $_INSTALL_ROOT/bin/amq_server_classes.icl\" failed"
    [ "$BOOM_TRACE" ] && echo "cp amq_server_channel.icl $_INSTALL_ROOT/bin/"
    cp amq_server_channel.icl $_INSTALL_ROOT/bin/ || croak "\"cp amq_server_channel.icl $_INSTALL_ROOT/bin/\" failed"
    [ "$BOOM_TRACE" ] && echo "chmod 0644 $_INSTALL_ROOT/bin/amq_server_channel.icl"
    chmod 0644 $_INSTALL_ROOT/bin/amq_server_channel.icl || croak "\"chmod 0644 $_INSTALL_ROOT/bin/amq_server_channel.icl\" failed"
    [ "$BOOM_TRACE" ] && echo "cp amq_broker.icl $_INSTALL_ROOT/bin/"
    cp amq_broker.icl $_INSTALL_ROOT/bin/ || croak "\"cp amq_broker.icl $_INSTALL_ROOT/bin/\" failed"
    [ "$BOOM_TRACE" ] && echo "chmod 0644 $_INSTALL_ROOT/bin/amq_broker.icl"
    chmod 0644 $_INSTALL_ROOT/bin/amq_broker.icl || croak "\"chmod 0644 $_INSTALL_ROOT/bin/amq_broker.icl\" failed"
    [ "$BOOM_TRACE" ] && echo "cp amq_peering.icl $_INSTALL_ROOT/bin/"
    cp amq_peering.icl $_INSTALL_ROOT/bin/ || croak "\"cp amq_peering.icl $_INSTALL_ROOT/bin/\" failed"
    [ "$BOOM_TRACE" ] && echo "chmod 0644 $_INSTALL_ROOT/bin/amq_peering.icl"
    chmod 0644 $_INSTALL_ROOT/bin/amq_peering.icl || croak "\"chmod 0644 $_INSTALL_ROOT/bin/amq_peering.icl\" failed"
    [ "$BOOM_TRACE" ] && echo "cp amq_peer_method.icl $_INSTALL_ROOT/bin/"
    cp amq_peer_method.icl $_INSTALL_ROOT/bin/ || croak "\"cp amq_peer_method.icl $_INSTALL_ROOT/bin/\" failed"
    [ "$BOOM_TRACE" ] && echo "chmod 0644 $_INSTALL_ROOT/bin/amq_peer_method.icl"
    chmod 0644 $_INSTALL_ROOT/bin/amq_peer_method.icl || croak "\"chmod 0644 $_INSTALL_ROOT/bin/amq_peer_method.icl\" failed"
    [ "$BOOM_TRACE" ] && echo "cp amq_peer_config.opf $_INSTALL_ROOT/bin/"
    cp amq_peer_config.opf $_INSTALL_ROOT/bin/ || croak "\"cp amq_peer_config.opf $_INSTALL_ROOT/bin/\" failed"
    [ "$BOOM_TRACE" ] && echo "chmod 0644 $_INSTALL_ROOT/bin/amq_peer_config.opf"
    chmod 0644 $_INSTALL_ROOT/bin/amq_peer_config.opf || croak "\"chmod 0644 $_INSTALL_ROOT/bin/amq_peer_config.opf\" failed"
    [ "$BOOM_TRACE" ] && echo "cp amq_peer_agent.smt $_INSTALL_ROOT/bin/"
    cp amq_peer_agent.smt $_INSTALL_ROOT/bin/ || croak "\"cp amq_peer_agent.smt $_INSTALL_ROOT/bin/\" failed"
    [ "$BOOM_TRACE" ] && echo "chmod 0644 $_INSTALL_ROOT/bin/amq_peer_agent.smt"
    chmod 0644 $_INSTALL_ROOT/bin/amq_peer_agent.smt || croak "\"chmod 0644 $_INSTALL_ROOT/bin/amq_peer_agent.smt\" failed"
    [ "$BOOM_TRACE" ] && echo "cp amq_exchange.icl $_INSTALL_ROOT/bin/"
    cp amq_exchange.icl $_INSTALL_ROOT/bin/ || croak "\"cp amq_exchange.icl $_INSTALL_ROOT/bin/\" failed"
    [ "$BOOM_TRACE" ] && echo "chmod 0644 $_INSTALL_ROOT/bin/amq_exchange.icl"
    chmod 0644 $_INSTALL_ROOT/bin/amq_exchange.icl || croak "\"chmod 0644 $_INSTALL_ROOT/bin/amq_exchange.icl\" failed"
    [ "$BOOM_TRACE" ] && echo "cp amq_binding_mgt.icl $_INSTALL_ROOT/bin/"
    cp amq_binding_mgt.icl $_INSTALL_ROOT/bin/ || croak "\"cp amq_binding_mgt.icl $_INSTALL_ROOT/bin/\" failed"
    [ "$BOOM_TRACE" ] && echo "chmod 0644 $_INSTALL_ROOT/bin/amq_binding_mgt.icl"
    chmod 0644 $_INSTALL_ROOT/bin/amq_binding_mgt.icl || croak "\"chmod 0644 $_INSTALL_ROOT/bin/amq_binding_mgt.icl\" failed"
    [ "$BOOM_TRACE" ] && echo "cp amq_queue.icl $_INSTALL_ROOT/bin/"
    cp amq_queue.icl $_INSTALL_ROOT/bin/ || croak "\"cp amq_queue.icl $_INSTALL_ROOT/bin/\" failed"
    [ "$BOOM_TRACE" ] && echo "chmod 0644 $_INSTALL_ROOT/bin/amq_queue.icl"
    chmod 0644 $_INSTALL_ROOT/bin/amq_queue.icl || croak "\"chmod 0644 $_INSTALL_ROOT/bin/amq_queue.icl\" failed"
    [ "$BOOM_TRACE" ] && echo "cp amq_connection.icl $_INSTALL_ROOT/bin/"
    cp amq_connection.icl $_INSTALL_ROOT/bin/ || croak "\"cp amq_connection.icl $_INSTALL_ROOT/bin/\" failed"
    [ "$BOOM_TRACE" ] && echo "chmod 0644 $_INSTALL_ROOT/bin/amq_connection.icl"
    chmod 0644 $_INSTALL_ROOT/bin/amq_connection.icl || croak "\"chmod 0644 $_INSTALL_ROOT/bin/amq_connection.icl\" failed"
    [ "$BOOM_TRACE" ] && echo "cp amq_queue_connection.icl $_INSTALL_ROOT/bin/"
    cp amq_queue_connection.icl $_INSTALL_ROOT/bin/ || croak "\"cp amq_queue_connection.icl $_INSTALL_ROOT/bin/\" failed"
    [ "$BOOM_TRACE" ] && echo "chmod 0644 $_INSTALL_ROOT/bin/amq_queue_connection.icl"
    chmod 0644 $_INSTALL_ROOT/bin/amq_queue_connection.icl || croak "\"chmod 0644 $_INSTALL_ROOT/bin/amq_queue_connection.icl\" failed"
    [ "$BOOM_TRACE" ] && echo "cp amq_queue_binding.icl $_INSTALL_ROOT/bin/"
    cp amq_queue_binding.icl $_INSTALL_ROOT/bin/ || croak "\"cp amq_queue_binding.icl $_INSTALL_ROOT/bin/\" failed"
    [ "$BOOM_TRACE" ] && echo "chmod 0644 $_INSTALL_ROOT/bin/amq_queue_binding.icl"
    chmod 0644 $_INSTALL_ROOT/bin/amq_queue_binding.icl || croak "\"chmod 0644 $_INSTALL_ROOT/bin/amq_queue_binding.icl\" failed"
    [ "$BOOM_TRACE" ] && echo "cp amq_connection_queue.icl $_INSTALL_ROOT/bin/"
    cp amq_connection_queue.icl $_INSTALL_ROOT/bin/ || croak "\"cp amq_connection_queue.icl $_INSTALL_ROOT/bin/\" failed"
    [ "$BOOM_TRACE" ] && echo "chmod 0644 $_INSTALL_ROOT/bin/amq_connection_queue.icl"
    chmod 0644 $_INSTALL_ROOT/bin/amq_connection_queue.icl || croak "\"chmod 0644 $_INSTALL_ROOT/bin/amq_connection_queue.icl\" failed"
    [ "$BOOM_TRACE" ] && echo "cp amq_resource.icl $_INSTALL_ROOT/bin/"
    cp amq_resource.icl $_INSTALL_ROOT/bin/ || croak "\"cp amq_resource.icl $_INSTALL_ROOT/bin/\" failed"
    [ "$BOOM_TRACE" ] && echo "chmod 0644 $_INSTALL_ROOT/bin/amq_resource.icl"
    chmod 0644 $_INSTALL_ROOT/bin/amq_resource.icl || croak "\"chmod 0644 $_INSTALL_ROOT/bin/amq_resource.icl\" failed"
    [ "$BOOM_TRACE" ] && echo "cp amq_server_base.cfg $_INSTALL_ROOT/bin/"
    cp amq_server_base.cfg $_INSTALL_ROOT/bin/ || croak "\"cp amq_server_base.cfg $_INSTALL_ROOT/bin/\" failed"
    [ "$BOOM_TRACE" ] && echo "chmod 0644 $_INSTALL_ROOT/bin/amq_server_base.cfg"
    chmod 0644 $_INSTALL_ROOT/bin/amq_server_base.cfg || croak "\"chmod 0644 $_INSTALL_ROOT/bin/amq_server_base.cfg\" failed"
    [ "$BOOM_TRACE" ] && echo "cp amq_federation.icl $_INSTALL_ROOT/bin/"
    cp amq_federation.icl $_INSTALL_ROOT/bin/ || croak "\"cp amq_federation.icl $_INSTALL_ROOT/bin/\" failed"
    [ "$BOOM_TRACE" ] && echo "chmod 0644 $_INSTALL_ROOT/bin/amq_federation.icl"
    chmod 0644 $_INSTALL_ROOT/bin/amq_federation.icl || croak "\"chmod 0644 $_INSTALL_ROOT/bin/amq_federation.icl\" failed"
    [ "$BOOM_TRACE" ] && echo "cp amq_failover.icl $_INSTALL_ROOT/bin/"
    cp amq_failover.icl $_INSTALL_ROOT/bin/ || croak "\"cp amq_failover.icl $_INSTALL_ROOT/bin/\" failed"
    [ "$BOOM_TRACE" ] && echo "chmod 0644 $_INSTALL_ROOT/bin/amq_failover.icl"
    chmod 0644 $_INSTALL_ROOT/bin/amq_failover.icl || croak "\"chmod 0644 $_INSTALL_ROOT/bin/amq_failover.icl\" failed"
    [ "$BOOM_TRACE" ] && echo "cp amq_console.icl $_INSTALL_ROOT/bin/"
    cp amq_console.icl $_INSTALL_ROOT/bin/ || croak "\"cp amq_console.icl $_INSTALL_ROOT/bin/\" failed"
    [ "$BOOM_TRACE" ] && echo "chmod 0644 $_INSTALL_ROOT/bin/amq_console.icl"
    chmod 0644 $_INSTALL_ROOT/bin/amq_console.icl || croak "\"chmod 0644 $_INSTALL_ROOT/bin/amq_console.icl\" failed"
    [ "$BOOM_TRACE" ] && echo "cp amq_console_schema.cml $_INSTALL_ROOT/bin/"
    cp amq_console_schema.cml $_INSTALL_ROOT/bin/ || croak "\"cp amq_console_schema.cml $_INSTALL_ROOT/bin/\" failed"
    [ "$BOOM_TRACE" ] && echo "chmod 0644 $_INSTALL_ROOT/bin/amq_console_schema.cml"
    chmod 0644 $_INSTALL_ROOT/bin/amq_console_schema.cml || croak "\"chmod 0644 $_INSTALL_ROOT/bin/amq_console_schema.cml\" failed"
    [ "$BOOM_TRACE" ] && echo "cp amq_console_schema.xml $_INSTALL_ROOT/bin/"
    cp amq_console_schema.xml $_INSTALL_ROOT/bin/ || croak "\"cp amq_console_schema.xml $_INSTALL_ROOT/bin/\" failed"
    [ "$BOOM_TRACE" ] && echo "chmod 0644 $_INSTALL_ROOT/bin/amq_console_schema.xml"
    chmod 0644 $_INSTALL_ROOT/bin/amq_console_schema.xml || croak "\"chmod 0644 $_INSTALL_ROOT/bin/amq_console_schema.xml\" failed"
    [ "$BOOM_TRACE" ] && echo "cp amq_console_config.icl $_INSTALL_ROOT/bin/"
    cp amq_console_config.icl $_INSTALL_ROOT/bin/ || croak "\"cp amq_console_config.icl $_INSTALL_ROOT/bin/\" failed"
    [ "$BOOM_TRACE" ] && echo "chmod 0644 $_INSTALL_ROOT/bin/amq_console_config.icl"
    chmod 0644 $_INSTALL_ROOT/bin/amq_console_config.icl || croak "\"chmod 0644 $_INSTALL_ROOT/bin/amq_console_config.icl\" failed"
    [ "$BOOM_TRACE" ] && echo "cp amq_cml.icl $_INSTALL_ROOT/bin/"
    cp amq_cml.icl $_INSTALL_ROOT/bin/ || croak "\"cp amq_cml.icl $_INSTALL_ROOT/bin/\" failed"
    [ "$BOOM_TRACE" ] && echo "chmod 0644 $_INSTALL_ROOT/bin/amq_cml.icl"
    chmod 0644 $_INSTALL_ROOT/bin/amq_cml.icl || croak "\"chmod 0644 $_INSTALL_ROOT/bin/amq_cml.icl\" failed"
    [ "$BOOM_TRACE" ] && echo "cp amq_server $_INSTALL_ROOT/bin/"
    cp amq_server $_INSTALL_ROOT/bin/ || croak "\"cp amq_server $_INSTALL_ROOT/bin/\" failed"
    [ "$BOOM_TRACE" ] && echo "chmod 0755 $_INSTALL_ROOT/bin/amq_server"
    chmod 0755 $_INSTALL_ROOT/bin/amq_server || croak "\"chmod 0755 $_INSTALL_ROOT/bin/amq_server\" failed"
    mkdir -p $_INSTALL_ROOT/doc/OpenAMQ_Server/
    [ "$BOOM_TRACE" ] && echo "cp readme.txt $_INSTALL_ROOT/doc/OpenAMQ_Server/"
    cp readme.txt $_INSTALL_ROOT/doc/OpenAMQ_Server/ || croak "\"cp readme.txt $_INSTALL_ROOT/doc/OpenAMQ_Server/\" failed"
    [ "$BOOM_TRACE" ] && echo "chmod 0644 $_INSTALL_ROOT/doc/OpenAMQ_Server/readme.txt"
    chmod 0644 $_INSTALL_ROOT/doc/OpenAMQ_Server/readme.txt || croak "\"chmod 0644 $_INSTALL_ROOT/doc/OpenAMQ_Server/readme.txt\" failed"
    mkdir -p $_INSTALL_ROOT/include/
    [ "$BOOM_TRACE" ] && echo "cp amq_server_config.h $_INSTALL_ROOT/include/"
    cp amq_server_config.h $_INSTALL_ROOT/include/ || croak "\"cp amq_server_config.h $_INSTALL_ROOT/include/\" failed"
    [ "$BOOM_TRACE" ] && echo "chmod 0644 $_INSTALL_ROOT/include/amq_server_config.h"
    chmod 0644 $_INSTALL_ROOT/include/amq_server_config.h || croak "\"chmod 0644 $_INSTALL_ROOT/include/amq_server_config.h\" failed"
    mkdir -p $_INSTALL_ROOT/bin/
    [ "$BOOM_TRACE" ] && echo "cp amq_server_config.txt $_INSTALL_ROOT/bin/"
    cp amq_server_config.txt $_INSTALL_ROOT/bin/ || croak "\"cp amq_server_config.txt $_INSTALL_ROOT/bin/\" failed"
    [ "$BOOM_TRACE" ] && echo "chmod 0644 $_INSTALL_ROOT/bin/amq_server_config.txt"
    chmod 0644 $_INSTALL_ROOT/bin/amq_server_config.txt || croak "\"chmod 0644 $_INSTALL_ROOT/bin/amq_server_config.txt\" failed"
    mkdir -p $_INSTALL_ROOT/include/
    [ "$BOOM_TRACE" ] && echo "cp amq_server_agent.h $_INSTALL_ROOT/include/"
    cp amq_server_agent.h $_INSTALL_ROOT/include/ || croak "\"cp amq_server_agent.h $_INSTALL_ROOT/include/\" failed"
    [ "$BOOM_TRACE" ] && echo "chmod 0644 $_INSTALL_ROOT/include/amq_server_agent.h"
    chmod 0644 $_INSTALL_ROOT/include/amq_server_agent.h || croak "\"chmod 0644 $_INSTALL_ROOT/include/amq_server_agent.h\" failed"
    [ "$BOOM_TRACE" ] && echo "cp amq_server_classes.h $_INSTALL_ROOT/include/"
    cp amq_server_classes.h $_INSTALL_ROOT/include/ || croak "\"cp amq_server_classes.h $_INSTALL_ROOT/include/\" failed"
    [ "$BOOM_TRACE" ] && echo "chmod 0644 $_INSTALL_ROOT/include/amq_server_classes.h"
    chmod 0644 $_INSTALL_ROOT/include/amq_server_classes.h || croak "\"chmod 0644 $_INSTALL_ROOT/include/amq_server_classes.h\" failed"
    [ "$BOOM_TRACE" ] && echo "cp amq_server_connection.h $_INSTALL_ROOT/include/"
    cp amq_server_connection.h $_INSTALL_ROOT/include/ || croak "\"cp amq_server_connection.h $_INSTALL_ROOT/include/\" failed"
    [ "$BOOM_TRACE" ] && echo "chmod 0644 $_INSTALL_ROOT/include/amq_server_connection.h"
    chmod 0644 $_INSTALL_ROOT/include/amq_server_connection.h || croak "\"chmod 0644 $_INSTALL_ROOT/include/amq_server_connection.h\" failed"
    [ "$BOOM_TRACE" ] && echo "cp amq_server_connection_table.h $_INSTALL_ROOT/include/"
    cp amq_server_connection_table.h $_INSTALL_ROOT/include/ || croak "\"cp amq_server_connection_table.h $_INSTALL_ROOT/include/\" failed"
    [ "$BOOM_TRACE" ] && echo "chmod 0644 $_INSTALL_ROOT/include/amq_server_connection_table.h"
    chmod 0644 $_INSTALL_ROOT/include/amq_server_connection_table.h || croak "\"chmod 0644 $_INSTALL_ROOT/include/amq_server_connection_table.h\" failed"
    [ "$BOOM_TRACE" ] && echo "cp amq_server_connection_list.h $_INSTALL_ROOT/include/"
    cp amq_server_connection_list.h $_INSTALL_ROOT/include/ || croak "\"cp amq_server_connection_list.h $_INSTALL_ROOT/include/\" failed"
    [ "$BOOM_TRACE" ] && echo "chmod 0644 $_INSTALL_ROOT/include/amq_server_connection_list.h"
    chmod 0644 $_INSTALL_ROOT/include/amq_server_connection_list.h || croak "\"chmod 0644 $_INSTALL_ROOT/include/amq_server_connection_list.h\" failed"
    mkdir -p $_INSTALL_ROOT/bin/
    [ "$BOOM_TRACE" ] && echo "cp amq_server_channel_agent.smt $_INSTALL_ROOT/bin/"
    cp amq_server_channel_agent.smt $_INSTALL_ROOT/bin/ || croak "\"cp amq_server_channel_agent.smt $_INSTALL_ROOT/bin/\" failed"
    [ "$BOOM_TRACE" ] && echo "chmod 0644 $_INSTALL_ROOT/bin/amq_server_channel_agent.smt"
    chmod 0644 $_INSTALL_ROOT/bin/amq_server_channel_agent.smt || croak "\"chmod 0644 $_INSTALL_ROOT/bin/amq_server_channel_agent.smt\" failed"
    mkdir -p $_INSTALL_ROOT/include/
    [ "$BOOM_TRACE" ] && echo "cp amq_server_channel_agent.h $_INSTALL_ROOT/include/"
    cp amq_server_channel_agent.h $_INSTALL_ROOT/include/ || croak "\"cp amq_server_channel_agent.h $_INSTALL_ROOT/include/\" failed"
    [ "$BOOM_TRACE" ] && echo "chmod 0644 $_INSTALL_ROOT/include/amq_server_channel_agent.h"
    chmod 0644 $_INSTALL_ROOT/include/amq_server_channel_agent.h || croak "\"chmod 0644 $_INSTALL_ROOT/include/amq_server_channel_agent.h\" failed"
    [ "$BOOM_TRACE" ] && echo "cp amq_server_channel.h $_INSTALL_ROOT/include/"
    cp amq_server_channel.h $_INSTALL_ROOT/include/ || croak "\"cp amq_server_channel.h $_INSTALL_ROOT/include/\" failed"
    [ "$BOOM_TRACE" ] && echo "chmod 0644 $_INSTALL_ROOT/include/amq_server_channel.h"
    chmod 0644 $_INSTALL_ROOT/include/amq_server_channel.h || croak "\"chmod 0644 $_INSTALL_ROOT/include/amq_server_channel.h\" failed"
    [ "$BOOM_TRACE" ] && echo "cp amq_server_channel_table.h $_INSTALL_ROOT/include/"
    cp amq_server_channel_table.h $_INSTALL_ROOT/include/ || croak "\"cp amq_server_channel_table.h $_INSTALL_ROOT/include/\" failed"
    [ "$BOOM_TRACE" ] && echo "chmod 0644 $_INSTALL_ROOT/include/amq_server_channel_table.h"
    chmod 0644 $_INSTALL_ROOT/include/amq_server_channel_table.h || croak "\"chmod 0644 $_INSTALL_ROOT/include/amq_server_channel_table.h\" failed"
    [ "$BOOM_TRACE" ] && echo "cp amq_server_resource.h $_INSTALL_ROOT/include/"
    cp amq_server_resource.h $_INSTALL_ROOT/include/ || croak "\"cp amq_server_resource.h $_INSTALL_ROOT/include/\" failed"
    [ "$BOOM_TRACE" ] && echo "chmod 0644 $_INSTALL_ROOT/include/amq_server_resource.h"
    chmod 0644 $_INSTALL_ROOT/include/amq_server_resource.h || croak "\"chmod 0644 $_INSTALL_ROOT/include/amq_server_resource.h\" failed"
    mkdir -p $_INSTALL_ROOT/bin/
    [ "$BOOM_TRACE" ] && echo "cp amq_broker_agent.smt $_INSTALL_ROOT/bin/"
    cp amq_broker_agent.smt $_INSTALL_ROOT/bin/ || croak "\"cp amq_broker_agent.smt $_INSTALL_ROOT/bin/\" failed"
    [ "$BOOM_TRACE" ] && echo "chmod 0644 $_INSTALL_ROOT/bin/amq_broker_agent.smt"
    chmod 0644 $_INSTALL_ROOT/bin/amq_broker_agent.smt || croak "\"chmod 0644 $_INSTALL_ROOT/bin/amq_broker_agent.smt\" failed"
    mkdir -p $_INSTALL_ROOT/include/
