    [ "$BOOM_TRACE" ] && echo "gsl -q -quiet:1 amq_server_channel.icl"
    gsl -q -quiet:1 amq_server_channel.icl || croak "\"gsl -q -quiet:1 amq_server_channel.icl\" failed"
