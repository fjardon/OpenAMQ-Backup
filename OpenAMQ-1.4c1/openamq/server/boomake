    [ "$BOOM_TRACE" ] && echo "cp amq_peer_method.h $_INSTALL_ROOT/include/"
    cp amq_peer_method.h $_INSTALL_ROOT/include/ || croak "\"cp amq_peer_method.h $_INSTALL_ROOT/include/\" failed"
    [ "$BOOM_TRACE" ] && echo "chmod 0644 $_INSTALL_ROOT/include/amq_peer_method.h"
    chmod 0644 $_INSTALL_ROOT/include/amq_peer_method.h || croak "\"chmod 0644 $_INSTALL_ROOT/include/amq_peer_method.h\" failed"
    [ "$BOOM_TRACE" ] && echo "cp amq_peer_config.h $_INSTALL_ROOT/include/"
    cp amq_peer_config.h $_INSTALL_ROOT/include/ || croak "\"cp amq_peer_config.h $_INSTALL_ROOT/include/\" failed"
    [ "$BOOM_TRACE" ] && echo "chmod 0644 $_INSTALL_ROOT/include/amq_peer_config.h"
    chmod 0644 $_INSTALL_ROOT/include/amq_peer_config.h || croak "\"chmod 0644 $_INSTALL_ROOT/include/amq_peer_config.h\" failed"
    mkdir -p $_INSTALL_ROOT/bin/
    [ "$BOOM_TRACE" ] && echo "cp amq_peer_config.txt $_INSTALL_ROOT/bin/"
    cp amq_peer_config.txt $_INSTALL_ROOT/bin/ || croak "\"cp amq_peer_config.txt $_INSTALL_ROOT/bin/\" failed"
    [ "$BOOM_TRACE" ] && echo "chmod 0644 $_INSTALL_ROOT/bin/amq_peer_config.txt"
    chmod 0644 $_INSTALL_ROOT/bin/amq_peer_config.txt || croak "\"chmod 0644 $_INSTALL_ROOT/bin/amq_peer_config.txt\" failed"
    mkdir -p $_INSTALL_ROOT/include/
    [ "$BOOM_TRACE" ] && echo "cp amq_peer_agent.h $_INSTALL_ROOT/include/"
