    c $MY_COPTS -L amq_server_resource_test || croak "\"c $MY_COPTS -L amq_server_resource_test\" failed"
    [ "$BOOM_TRACE" ] && echo "c $MY_COPTS -L amq_broker_test"
    c $MY_COPTS -L amq_broker_test || croak "\"c $MY_COPTS -L amq_broker_test\" failed"
    [ "$BOOM_TRACE" ] && echo "c $MY_COPTS -L amq_peering_test"
    c $MY_COPTS -L amq_peering_test || croak "\"c $MY_COPTS -L amq_peering_test\" failed"
    [ "$BOOM_TRACE" ] && echo "c $MY_COPTS -L amq_peer_method_test"
    c $MY_COPTS -L amq_peer_method_test || croak "\"c $MY_COPTS -L amq_peer_method_test\" failed"
    [ "$BOOM_TRACE" ] && echo "c $MY_COPTS -L amq_peer_config_test"
    c $MY_COPTS -L amq_peer_config_test || croak "\"c $MY_COPTS -L amq_peer_config_test\" failed"
    [ "$BOOM_TRACE" ] && echo "c $MY_COPTS -L amq_exchange_system_test"
    c $MY_COPTS -L amq_exchange_system_test || croak "\"c $MY_COPTS -L amq_exchange_system_test\" failed"
    [ "$BOOM_TRACE" ] && echo "c $MY_COPTS -L amq_exchange_fanout_test"
    c $MY_COPTS -L amq_exchange_fanout_test || croak "\"c $MY_COPTS -L amq_exchange_fanout_test\" failed"
    [ "$BOOM_TRACE" ] && echo "c $MY_COPTS -L amq_exchange_direct_test"
    c $MY_COPTS -L amq_exchange_direct_test || croak "\"c $MY_COPTS -L amq_exchange_direct_test\" failed"
    [ "$BOOM_TRACE" ] && echo "c $MY_COPTS -L amq_exchange_topic_test"
    c $MY_COPTS -L amq_exchange_topic_test || croak "\"c $MY_COPTS -L amq_exchange_topic_test\" failed"
    [ "$BOOM_TRACE" ] && echo "c $MY_COPTS -L amq_exchange_headers_test"
    c $MY_COPTS -L amq_exchange_headers_test || croak "\"c $MY_COPTS -L amq_exchange_headers_test\" failed"
    [ "$BOOM_TRACE" ] && echo "c $MY_COPTS -L amq_exchange_regexp_test"
    c $MY_COPTS -L amq_exchange_regexp_test || croak "\"c $MY_COPTS -L amq_exchange_regexp_test\" failed"
    [ "$BOOM_TRACE" ] && echo "c $MY_COPTS -L amq_exchange_test"
    c $MY_COPTS -L amq_exchange_test || croak "\"c $MY_COPTS -L amq_exchange_test\" failed"
    [ "$BOOM_TRACE" ] && echo "c $MY_COPTS -L amq_exchange_table_test"
    c $MY_COPTS -L amq_exchange_table_test || croak "\"c $MY_COPTS -L amq_exchange_table_test\" failed"
    [ "$BOOM_TRACE" ] && echo "c $MY_COPTS -L amq_exchange_list_test"
    c $MY_COPTS -L amq_exchange_list_test || croak "\"c $MY_COPTS -L amq_exchange_list_test\" failed"
    [ "$BOOM_TRACE" ] && echo "c $MY_COPTS -L amq_binding_test"
    c $MY_COPTS -L amq_binding_test || croak "\"c $MY_COPTS -L amq_binding_test\" failed"
