# -----------------------   CHECK BINARY FILES EXIST   ------------------------

a_check_binary_files_exist() {
:
    if [ ! -f "libamq_server.a" ]; then
        croak "libamq_server.a is missing"
    fi
    if [ ! -f "amq_server_agent.smt" ]; then
        croak "amq_server_agent.smt is missing"
    fi
    if [ ! -f "amq_peer_agent.smt" ]; then
        croak "amq_peer_agent.smt is missing"
    fi
    if [ ! -f "amq_server_base.cfg" ]; then
        croak "amq_server_base.cfg is missing"
    fi
    if [ ! -f "amq_console_schema.cml" ]; then
        croak "amq_console_schema.cml is missing"
    fi
    if [ ! -f "amq_console_schema.xml" ]; then
        croak "amq_console_schema.xml is missing"
    fi
    if [ ! -f "amq_server" ]; then
        croak "amq_server is missing"
    fi
    if [ ! -f "readme.txt" ]; then
        croak "readme.txt is missing"
    fi
    if [ ! -f "amq_server_config.h" ]; then
        croak "amq_server_config.h is missing"
    fi
    if [ ! -f "amq_server_config.txt" ]; then
        croak "amq_server_config.txt is missing"
    fi
    if [ ! -f "amq_server_agent.h" ]; then
        croak "amq_server_agent.h is missing"
    fi
    if [ ! -f "amq_server_classes.h" ]; then
        croak "amq_server_classes.h is missing"
    fi
    if [ ! -f "amq_server_connection.h" ]; then
        croak "amq_server_connection.h is missing"
    fi
