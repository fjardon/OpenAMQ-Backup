    [ "$BOOM_TRACE" ] && echo "c $MY_COPTS -L amq_queue_bindings_list_test"
    c $MY_COPTS -L amq_queue_bindings_list_test || croak "\"c $MY_COPTS -L amq_queue_bindings_list_test\" failed"
    [ "$BOOM_TRACE" ] && echo "c $MY_COPTS -L amq_queue_bindings_list_table_test"
    c $MY_COPTS -L amq_queue_bindings_list_table_test || croak "\"c $MY_COPTS -L amq_queue_bindings_list_table_test\" failed"
    [ "$BOOM_TRACE" ] && echo "c $MY_COPTS -L amq_hitset_test"
    c $MY_COPTS -L amq_hitset_test || croak "\"c $MY_COPTS -L amq_hitset_test\" failed"
    [ "$BOOM_TRACE" ] && echo "c $MY_COPTS -L amq_index_test"
    c $MY_COPTS -L amq_index_test || croak "\"c $MY_COPTS -L amq_index_test\" failed"
