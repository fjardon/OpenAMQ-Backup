    [ "$BOOM_TRACE" ] && echo "gsl -q -quiet:1 amq_queue_binding_agent.smt"
    gsl -q -quiet:1 amq_queue_binding_agent.smt || croak "\"gsl -q -quiet:1 amq_queue_binding_agent.smt\" failed"
    [ "$BOOM_TRACE" ] && echo "gsl -q -quiet:1 amq_connection_queue_agent.smt"
    gsl -q -quiet:1 amq_connection_queue_agent.smt || croak "\"gsl -q -quiet:1 amq_connection_queue_agent.smt\" failed"
    [ "$BOOM_TRACE" ] && echo "gsl -q -quiet:1 amq_federation_agent.smt"
    gsl -q -quiet:1 amq_federation_agent.smt || croak "\"gsl -q -quiet:1 amq_federation_agent.smt\" failed"
    [ "$BOOM_TRACE" ] && echo "gsl -q -quiet:1 amq_failover_agent.smt"
    gsl -q -quiet:1 amq_failover_agent.smt || croak "\"gsl -q -quiet:1 amq_failover_agent.smt\" failed"
    [ "$BOOM_TRACE" ] && echo "gsl -q -quiet:1 amq_console_agent.smt"
    gsl -q -quiet:1 amq_console_agent.smt || croak "\"gsl -q -quiet:1 amq_console_agent.smt\" failed"
    [ "$BOOM_TRACE" ] && echo "gsl -q -quiet:1 amq_console_config_agent.smt"
    gsl -q -quiet:1 amq_console_config_agent.smt || croak "\"gsl -q -quiet:1 amq_console_config_agent.smt\" failed"
    echo "Source files successfully generated">stamp_generate
}

# ------------------------   PRODUCE GENERATED FILES   ------------------------

a_produce_generated_files() {
:
    carp "Generating files..."

    gsl -q amq_server_config.opf
    gsl -q amq_console_schema.gsl
    perl -S ipr_resource.pl @amq_server_resource.rc
    
    [ "$BOOM_TRACE" ] && echo "gsl -q -quiet:1 amq_server.asl"
    gsl -q -quiet:1 amq_server.asl || croak "\"gsl -q -quiet:1 amq_server.asl\" failed"
    [ "$BOOM_TRACE" ] && echo "gsl -q -quiet:1 amq_server_config.opf"
    gsl -q -quiet:1 amq_server_config.opf || croak "\"gsl -q -quiet:1 amq_server_config.opf\" failed"
    [ "$BOOM_TRACE" ] && echo "gsl -q -quiet:1 amq_server_agent.smt"
    gsl -q -quiet:1 amq_server_agent.smt || croak "\"gsl -q -quiet:1 amq_server_agent.smt\" failed"
    [ "$BOOM_TRACE" ] && echo "gsl -q -quiet:1 amq_server_classes.icl"
    gsl -q -quiet:1 amq_server_classes.icl || croak "\"gsl -q -quiet:1 amq_server_classes.icl\" failed"
    [ "$BOOM_TRACE" ] && echo "gsl -q -quiet:1 amq_server_connection.icl"
    gsl -q -quiet:1 amq_server_connection.icl || croak "\"gsl -q -quiet:1 amq_server_connection.icl\" failed"
    [ "$BOOM_TRACE" ] && echo "gsl -q -quiet:1 amq_server_connection_table.icl"
    gsl -q -quiet:1 amq_server_connection_table.icl || croak "\"gsl -q -quiet:1 amq_server_connection_table.icl\" failed"
    [ "$BOOM_TRACE" ] && echo "gsl -q -quiet:1 amq_server_connection_list.icl"
    gsl -q -quiet:1 amq_server_connection_list.icl || croak "\"gsl -q -quiet:1 amq_server_connection_list.icl\" failed"
    [ "$BOOM_TRACE" ] && echo "gsl -q -quiet:1 amq_server_channel.icl"
    gsl -q -quiet:1 amq_server_channel.icl || croak "\"gsl -q -quiet:1 amq_server_channel.icl\" failed"
    [ "$BOOM_TRACE" ] && echo "gsl -q -quiet:1 amq_server_channel_table.icl"
    gsl -q -quiet:1 amq_server_channel_table.icl || croak "\"gsl -q -quiet:1 amq_server_channel_table.icl\" failed"
    [ "$BOOM_TRACE" ] && echo "gsl -q -quiet:1 amq_server_resource.icl"
    gsl -q -quiet:1 amq_server_resource.icl || croak "\"gsl -q -quiet:1 amq_server_resource.icl\" failed"
