    c $MY_COPTS -L amq_index_array_test || croak "\"c $MY_COPTS -L amq_index_array_test\" failed"
    [ "$BOOM_TRACE" ] && echo "c $MY_COPTS -L amq_queue_test"
    c $MY_COPTS -L amq_queue_test || croak "\"c $MY_COPTS -L amq_queue_test\" failed"
    [ "$BOOM_TRACE" ] && echo "c $MY_COPTS -L amq_queue_table_test"
    c $MY_COPTS -L amq_queue_table_test || croak "\"c $MY_COPTS -L amq_queue_table_test\" failed"
    [ "$BOOM_TRACE" ] && echo "c $MY_COPTS -L amq_queue_list_test"
    c $MY_COPTS -L amq_queue_list_test || croak "\"c $MY_COPTS -L amq_queue_list_test\" failed"
    [ "$BOOM_TRACE" ] && echo "c $MY_COPTS -L amq_queue_set_test"
    c $MY_COPTS -L amq_queue_set_test || croak "\"c $MY_COPTS -L amq_queue_set_test\" failed"
    [ "$BOOM_TRACE" ] && echo "c $MY_COPTS -L amq_queue_basic_test"
    c $MY_COPTS -L amq_queue_basic_test || croak "\"c $MY_COPTS -L amq_queue_basic_test\" failed"
    [ "$BOOM_TRACE" ] && echo "c $MY_COPTS -L amq_connection_test"
    c $MY_COPTS -L amq_connection_test || croak "\"c $MY_COPTS -L amq_connection_test\" failed"
    [ "$BOOM_TRACE" ] && echo "c $MY_COPTS -L amq_connection_by_broker_test"
    c $MY_COPTS -L amq_connection_by_broker_test || croak "\"c $MY_COPTS -L amq_connection_by_broker_test\" failed"
    [ "$BOOM_TRACE" ] && echo "c $MY_COPTS -L amq_consumer_test"
    c $MY_COPTS -L amq_consumer_test || croak "\"c $MY_COPTS -L amq_consumer_test\" failed"
