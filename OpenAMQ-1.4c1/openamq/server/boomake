    c $MY_COPTS -L amq_consumer_basic_test || croak "\"c $MY_COPTS -L amq_consumer_basic_test\" failed"
    [ "$BOOM_TRACE" ] && echo "c $MY_COPTS -L amq_queue_connection_test"
    c $MY_COPTS -L amq_queue_connection_test || croak "\"c $MY_COPTS -L amq_queue_connection_test\" failed"
    [ "$BOOM_TRACE" ] && echo "c $MY_COPTS -L amq_queue_binding_test"
    c $MY_COPTS -L amq_queue_binding_test || croak "\"c $MY_COPTS -L amq_queue_binding_test\" failed"
    [ "$BOOM_TRACE" ] && echo "c $MY_COPTS -L amq_connection_queue_test"
    c $MY_COPTS -L amq_connection_queue_test || croak "\"c $MY_COPTS -L amq_connection_queue_test\" failed"
    [ "$BOOM_TRACE" ] && echo "c $MY_COPTS -L amq_resource_test"
    c $MY_COPTS -L amq_resource_test || croak "\"c $MY_COPTS -L amq_resource_test\" failed"
    [ "$BOOM_TRACE" ] && echo "c $MY_COPTS -L amq_server_method_test"
    c $MY_COPTS -L amq_server_method_test || croak "\"c $MY_COPTS -L amq_server_method_test\" failed"
